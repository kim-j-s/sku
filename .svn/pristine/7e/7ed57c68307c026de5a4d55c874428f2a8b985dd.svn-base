@charset "UTF-8";
/* font */
@font-face { font-family: 'notosanskr'; font-style: normal; font-weight: 400; src: url("../font/NotoSansKR-Regular.eot"); src: url("../font/NotoSansKR-Regular.woff2") format("woff2"), url("../font/NotoSansKR-Regular.woff") format("woff"); }

@font-face { font-family: 'notosanskr'; font-style: normal; font-weight: 700; src: url("../font/NotoSansKR-Bold.eot"); src: url("../font/NotoSansKR-Bold.woff2") format("woff2"), url("../font/NotoSansKR-Bold.woff") format("woff"); }

/* reset */
html, body, form, div, p, h1, h2, h3, h4, h5, h6, dl, dt, dd, ul, ol, li, fieldset, th, td, button { margin: 0; padding: 0; border: 0; font: inherit; -webkit-box-sizing:border-box; box-sizing:border-box;}

html, body { height: 100%; font-size: 16px; }

body { -webkit-text-size-adjust: none; font-family: 'notosanskr','Apple SD Gothic Neo','AppleGothic','Apple Gothic','-apple-system',sans-serif; font-weight: 400; color: #323232; word-break: keep-all; }

input, textarea, select, button { font: inherit; vertical-align: middle; padding: 0; margin: 0; color: #222; font-family: 'notosanskr','Apple SD Gothic Neo','AppleGothic','Apple Gothic','-apple-system',sans-serif; -webkit-box-sizing:border-box; box-sizing:border-box;}

input[type="button"], input[type="text"], input[type="image"], input[type="submit"], input[type="password"], textarea { outline: none; -webkit-appearance: none; }

input[type="button"]:hover, input[type="submit"]:hover { cursor: pointer; }

button:focus, a:focus { outline: none; }

textarea { resize: none; }

table { width: 100%; border-collapse: collapse; border-spacing: 0; }

ul, ol, li { list-style-type: none; -webkit-box-sizing: border-box; box-sizing: border-box;}

img, fieldset, iframe { border: none; }

address, cite, code, dfn, em, var, th, strong { font-style: normal; font-weight: 400; }

a, a:active, a:hover { text-decoration: none; color: inherit; font-weight: 400; }

a:hover { cursor: pointer !important; }

article, aside, dialog, figure, footer, header, hgroup, nav, section { display: block; -webkit-box-sizing:border-box; box-sizing:border-box;}

caption, legend { height: 0; width: 0; line-height: 0; font-size: 0; overflow: hidden; visibility: hidden; display: none; -webkit-padding-start: 0; -webkit-padding-end: 0; }

button { background: transparent; cursor: pointer; }

iframe { border: 0; }

input::-ms-clear { display: none; }

::-webkit-input-placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

::-moz-placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

:-ms-input-placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

::-ms-input-placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

::placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

:-ms-input-placeholder { color: #999 !important; opacity: 1 !important; font-weight: 400; font-family: "notosanskr","Apple SD Gothic Neo","AppleGothic","Apple Gothic","-apple-system",sans-serif; }

img { border: none; }

select { background: transparent; -webkit-appearance: none; -moz-appearance: none; appearance: none;}

select::-ms-expand { display: none; }

.ellipsis { display: block; overflow: hidden; -o-text-overflow: ellipsis; text-overflow: ellipsis; white-space: nowrap; }

.blind { position: absolute; overflow: hidden; clip: rect(0 0 0 0); margin: -1px; width: 1px; height: 1px; }


/* common */
.dpi { display:inline !important;}
.dpb { display:block !important;}
.dpib { display:inline-block !important;}
.dpn { display:none !important;}


/* common element */
/* iput */
.inputBox {position:relative; display:inline-block;}
.inputBox.full {width:100% !important;}
.inputBox .inp {display:block; width:100%; height:35px; padding:0 15px; font-size:14px; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s; background:transparent;}
.inputBox .inp:disabled {}
.inputBox .inp:focus {border:1px solid #569df6;}
.inputBox.sch {position:static; border-radius:3px;}
.inputBox.sch .inp {padding-right:40px; border:1px solid #f4f4f4; border-radius:3px; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.inputBox.sch .inp.on {padding-right:70px;}
.inputBox.sch .inp {background:transparent;}
.inputBox.sch .inp:focus {background:#f4f4f4; border:1px solid #569df6;}
.inputBox.sch .schBtn {position:absolute; top:5px; right:10px;}
.inputBox.sch.on {z-index:2;}
.inputBox.sch.on .inp {background:#f4f4f4;}
.inputBox.sch .inpReset {position:absolute; top:5px; right:40px; opacity:0; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}

/* input checkbox switch */
.switch {display:inline-block; font-size:0; width:44px; height:24px; border-radius:12px;}
.switchCheckbox {position:relative; width:44px; height:24px; border-radius:12px; outline:none; background:#d9d9d9; -webkit-transition:all .2s ease-in-out; -o-transition:all .2s ease-in-out; transition:all .2s ease-in-out;}
.switchCheckbox {-webkit-appearance: none; -moz-appearance: none; appearance: none;}
.switchCheckbox:checked {background:#3e8ae9;}
.switchCheckbox:after {content:''; position:absolute; top:50%; left:3px; width:18px; height:18px; margin-top:-9px; border-radius:50%; background:#fff; -webkit-box-shadow: 0px 1px 4px 1px rgba(0,0,0,0.2); box-shadow: 0px 1px 4px 1px rgba(0,0,0,0.2); -webkit-transition: all .2s ease-in-out; -o-transition: all .2s ease-in-out; transition: all .2s ease-in-out;}
.switchCheckbox:checked:after {left:-webkit-calc(100% - 21px);left:calc(100% - 21px);}

/* button */
.btn {display:inline-block; font-size:0;}
.btn.schBtn {width:25px; height:25px; background:url(../img/icon_gnb_search_50_px.svg) left top no-repeat; background-size:100%;}
.btn.close {position:relative; width:24px; height:24px; padding:5px; overflow:hidden;}
.btn.close:before {content:''; position:absolute; top:11px; left:3px; width:18px; border-bottom:2px solid #000; -webkit-transform: rotate(45deg); -ms-transform: rotate(45deg); -o-transform: rotate(45deg); transform: rotate(45deg);}
.btn.close:after {content:''; position:absolute; top:11px; right:3px; width:18px; border-bottom:2px solid #000; -webkit-transform: rotate(135deg); -ms-transform: rotate(135deg); -o-transform: rotate(135deg); transform: rotate(135deg);}
.btn.close2 {width:35px; height:35px; overflow:hidden; text-indent:-9999px; background:url(../img/icon_close_w_70_px.svg) left top no-repeat; background-size:contain;}
.btn.inpReset {width:25px; height:25px; background:url(../img/btn_input_del_selected.svg) left top no-repeat; background-size:contain;}
.btn.inpReset.on {opacity:1;}
.btn.scMenuBtn {width:30px; height:30px; overflow:hidden; text-indent:-9999px; background:url(../img/btn_title_shot_white_60_px.svg) left top no-repeat; background-size:contain; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.btn.pageBack {width:30px; height:30px; overflow:hidden; text-indent:-9999px; background:url(../img/btn_title_arrow_white_60_px.svg) left top no-repeat; background-size:contain; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}

.btn.scBtn {position:relative; display:block; height:18px; line-height:18px; padding-left:26px; font-weight:700; font-size:12px; color:#222;}
.btn.scBtn:before {content:''; position:absolute; top:1px; left:0; width:16px; height:16px;}
.btn.scBtn.ty1:before {background:url(../img/icon_add_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty2:before {background:url(../img/icon_share_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty3:before {background:url(../img/icon_survey_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty4:before {background:url(../img/icon_download_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty5:before {background:url(../img/icon_delete_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty6:before {background:url(../img/icon_complete_32_px.svg) left top no-repeat; background-size:contain;}

.btn.gototop {width:34px; height:34px; border:1px solid #6280b2; border-radius:4px; background:#fff; -webkit-box-shadow: 0px 0px 3px 0px rgba(189,199,224,1); box-shadow: 0px 0px 3px 0px rgba(189,199,224,1);}
.btn.gototop > span {position:relative; display:block; padding-top:14px; font-weight:700; font-size:10px; color:#222; line-height:14px; text-align:center;}
.btn.gototop > span:before {content:''; position:absolute; top:0; left:50%; margin-left:-7px; width:14px; height:14px; background:url(../img/icon_top_28_px.svg) center center no-repeat; background-size:contain;}

.btn.channelBtn {display:inline-block; margin:0 4px 10px 4px; padding:1px 12px; line-height:21px; font-size:14px; color:#233b6a; border:1px solid #3e8ae9; border-radius:12px;}
.btn.channelBtn.on {color:#fff; background:#3e8ae9;}

.btn.sort {text-align:center;}
.btn.sort > span {display:inline-block; vertical-align:middle; padding-left:3px; font-weight:700; font-size:14px; color:#6c788f;}
.btn.sort.ty1:before {content:''; display:inline-block; width:20px; height:20px; background:url(../img/icon_list_filter_40_px.svg) left top no-repeat; background-size:contain; vertical-align:middle;}
.btn.sort.ty2:before {content:''; display:inline-block; width:20px; height:20px; background:url(../img/icon_list_solting_40_px.svg) left top no-repeat; background-size:contain; vertical-align:middle;}

/* selectbox */
.selectbox {display:inline-block; height:30px; padding-right:20px; border:1px solid #fff; border-radius:0;}
.selectbox {background:url(../img/icon_arrow_open_28_px.svg) right center no-repeat; background-size:14px 14px;}

/* ----------- kjs ----------- */
header {position:relative; z-index:6; height:55px;}
header h1 {position:absolute; top:0; left:0; width:100%; height:100%; z-index:-1; border-radius:3px; overflow:hidden; text-indent:-9999px; background:#f4f4f4;}
header h1:before {content:''; position:absolute; top:12px; left:15px; width:81px; height:14px; background:url(../img/logo.svg) center center no-repeat; background-size:100%;}
header h1.on:before {opacity:0; -webkit-transition:1s; -o-transition:1s; transition:1s;}

header .keyWord {position:absolute; top:0; left:0; width:100%; height:100%; z-index:-2; padding:9px 40px 8px 15px; border-radius:3px; overflow:hidden; -o-text-overflow:ellipsis; text-overflow:ellipsis; white-space:nowrap; font-size:12px; color:#666; line-height:18px; background:#f4f4f4;}
header .keyWord > span {color:#e1002a;}
header .keyWord.on {z-index:-1;}

/* header Type Main */
.header {position:fixed; top:0; left:0; z-index:1; width:100%; padding:10px 15px 10px 53px; border-bottom:1px solid #c8d0e0; background:#fff; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.header.on {-webkit-box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}
.header > .inner {position:relative;}

/* detail header */
.detailHeader {position:fixed; top:0; left:0; z-index:1; width:100%; height:55px; padding:12px 50px 12px 45px; border-bottom:1px solid #646872; background:#45484f; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.detailHeader .btn.pageBack {position:absolute; top:12px; left:15px;}
.detailHeader .btn.scMenuBtn {}
.detailHeader .keyWord {position:static; padding:0 5px 0 0; color:#fff; line-height:30px; background:#45484f; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.detailHeader.on {border-bottom:1px solid #c8d0e0; -webkit-box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2); box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2); background:#fff;}
.detailHeader.on.none {-webkit-box-shadow:none;box-shadow:none;}
.detailHeader.on .btn.scMenuBtn {background:url(../img/btn_title_shot_black_60_px.svg) left top no-repeat; background-size:contain;}
.detailHeader.on .btn.pageBack {background:url(../img/btn_title_arrow_black_60_px.svg) left top no-repeat; background-size:contain;}
.detailHeader.on .keyWord {position:static; padding:0 5px 0 0; color:#222; line-height:30px; background:#fff;}

/* list header */
.listHeader {position:fixed; top:0; left:0; z-index:1; width:100%; height:55px; padding:12px 15px 12px 45px; border-bottom:1px solid #c8d0e0; background:#fff; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.listHeader .btn.pageBack {position:absolute; top:12px; left:15px; background:url(../img/btn_title_arrow_black_60_px.svg) left top no-repeat; background-size:contain;}
.listHeader .keyWord {position:static; padding:0; font-weight:700; font-size:16px; color:#222; line-height:30px; background:#fff;}
.listHeader.on {-webkit-box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}

/* gnb */
.gnbOpen {position:absolute; top:10px; left:15px; width:24px; height:35px; overflow:hidden; text-indent:-9999px; background:url(../img/menu_normal.svg) left top no-repeat; background-size:cover;}
nav {position:fixed; top:0; right:0; bottom:0; left:-100%; width:100%; z-index:101; overflow:auto; background:#fff; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
nav.on {left:0;}
.navTop {position:relative; z-index:1; height:55px; background:#2d2d2d;}
.navTop:before {content:''; position:absolute; top:22px; left:20px; width:81px; height:14px; background:url(../img/icon_menu_logo.svg) center center no-repeat; background-size:contain;}
.navTop .btn.close2 {position:absolute; top:10px; right:20px;}

.gnb {position:absolute; top:0; left:0; padding-top:55px; overflow:auto; width:100%; height:100%;}
.gnbList {}
.gnbList > li {position:relative; padding:15px 20px 15px 45px;}
.gnbList > li:before {content:''; position:absolute; top:15px; left:20px; width:20px; height:20px;}
.gnbList > li.ty1:before {background:url(../img/icon_menu_home_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty2:before {background:url(../img/icon_menu_learning_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty3:before {background:url(../img/icon_menu_recommend_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty4:before {background:url(../img/icon_menu_comm_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty5:before {background:url(../img/icon_menu_support_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty6:before {background:url(../img/icon_menu_search_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty7:before {background:url(../img/icon_menu_my_40_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li + li {border-top:1px solid #c8d0e0;}
.gnbList > li > span {display:block; line-height:20px; font-weight:700; font-size:16px; color:#222;}
.gnbList > li > a {position:relative; display:block; line-height:20px; font-weight:700; font-size:16px; color:#222;}
.gnbList > li > a:after {content:''; position:absolute; top:-2px; right:0; width:25px; height:25px; background:url(../img/icon_arrow_go_50_px.svg) center center no-repeat; background-size:contain;}

.gnbListDept {padding:10px 0 5px 0;}
.gnbListDept > li {}
.gnbListDept > li + li {margin-top:10px;}
.gnbListDept > li a {display:block; font-size:14px; color:#222;}
.gnbListDept > li a span {font-size:12px; color:#999;}

/* content */
.content {}

/* shortCut */
.shortCuts {position:absolute; top:12px; right:20px; z-index:1;}
.shortCutList {position:absolute; top:30px; right:-5px; display:none; width:180px; border-radius:8px; background:#fff; -webkit-box-shadow: 0px 4px 8px 0px rgba(34,34,34,0.53); box-shadow: 0px 4px 8px 0px rgba(34,34,34,0.53);}
.shortCutList > li {padding:13px 0 13px 15px;}
.shortCutList > li + li {border-top:1px solid #ddd;}

/* 공지사항 */
.noticeWrap {position:relative; padding:16px 56px 16px 15px; overflow:hidden; background:#fff;}
.noticeWrap.on {height:0; padding:0 40px 0 15px; border:none; -webkit-transition:.5s; -o-transition:.5s; transition:.5s;}
.noticeWrap > a {position:relative; display:block; padding-left:27px; line-height:16px; font-size:12px; color:#666; overflow: hidden; -o-text-overflow: ellipsis; text-overflow: ellipsis; white-space: nowrap;}
.noticeWrap > a:before {content:''; position:absolute; top:0; left:0; width:16px; height:16px; background:url(../img/notice.svg) center center no-repeat; background-size:100%;}
.noticeWrap > .btn.close {position:absolute; top:13px; right:16px;}

/* Toolbar */
.toolBar {position:fixed; left:0; bottom:0; z-index:5; width:100%; height:55px; background:rgba(45,45,45,0.95);}
.toolList {display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;}
.toolList > li {-webkit-box-flex:1;-webkit-flex:1;-ms-flex:1;flex:1; padding-top:10px;}
.toolList > li > a {position:relative; display:block; padding-top:21px; text-align:center; font-size:10px; color:#fff; line-height:14px;}
.toolList > li > a:before {content:''; position:absolute; top:0; left:50%; margin-left:-10px; width:20px; height:20px;}
.toolList > li.new > a:after {content:''; position:absolute; top:0; left:50%; margin-left:5px; width:10px; height:10px; background:url(../img/new_badge.svg) center center no-repeat; background-size:contain;}
.toolList > li.on > a  {font-weight:700;}
.toolList > li.ty1 > a:before {background:url(../img/icon_home_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty1.on > a:before {background:url(../img/icon_home_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty2 > a:before {background:url(../img/icon_learning_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty2.on > a:before {background:url(../img/icon_learning_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty3 > a:before {background:url(../img/icon_comm_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty3.on > a:before {background:url(../img/icon_comm_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty4 > a:before {background:url(../img/icon_notice_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty4.on > a:before {background:url(../img/icon_notice_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty5 > a:before {background:url(../img/icon_40_px_bottom_my_normal.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty5.on > a:before {background:url(../img/icon_my_select_40_px.svg) center center no-repeat; background-size:contain;}

/* page top */
.pageTop {position:fixed; bottom:75px; right:10px; z-index:1; opacity:0; -webkit-transition:0.3s; -o-transition:0.3s; transition:0.3s;}
.pageTop.on {opacity:1;}

/* 메인 - 추천 과정 */
.recCourseWrap {position:relative; padding-top:30px; background:#6280b2;}
.recCourseWrap > .tit {padding:0 0 15px 15px; font-size:16px; color:#fff; letter-spacing:-0.35px; line-height:24px;}
.recCourseWrap > .tit strong {font-weight:700;}

.recCourse {overflow:hidden;}
.recCourse .swiper-slide {padding:0 15px 60px 15px;}
.recCourse.swiper-container-initialized .swiper-slide {padding:0 15px 20px 15px;}
.recCourse .swiper-slide .txt {padding:0 0 12px 0; font-size:0;}
.recCourse .swiper-slide .txt a {position:relative; display:inline-block; padding-right:17px; font-size:12px; color:#fff; line-height:18px; letter-spacing:-0.2px;}
.recCourse .swiper-slide .txt a:after {content:''; position:absolute; top:2px; right:0; width:14px; height:14px; background:url(../img/icon_arrow_white_28_px.svg) center center no-repeat; background-size:contain;}
.recCourse .swiper-slide .txt strong {font-weight:700;}
.recCourse .card {margin:0; -webkit-box-shadow: 0px 1px 5px 0px rgba(72,106,162,1); box-shadow: 0px 1px 5px 0px rgba(72,106,162,1);}
.recCourse .swiper-pagination {padding-bottom:25px;}

/* 메인 - 추천 과정 목록 */
.mainRec {}
.recList {}
.recList > li {}
.recList > li .recTit {padding:40px 0 15px 15px;}
.recList > li .recTit a {position:relative; display:inline-block; padding-right:16px; font-weight:700; font-size:14px; color:#222; line-height:20px; letter-spacing:-0.3px;}
.recList > li .recTit a:after {content:''; position:relative; top:2px; display:inline-block; width:14px; height:14px; background:url(../img/icon_arrow_black_28_px.svg) center center no-repeat; background-size:contain;}
.recList > li .recTit a .cName {font-weight:700; color:#4072c6; display:inline-block; padding-bottom:1px; color:#4072c6;}
.recList > li .recTit a .txt {display:inline-block;}
.recList > li .card:first-child {margin-top:30px;}
.recList > li .emptyBox {padding:60px 15px 50px 15px;}
.recList > li .btnListMore {margin-bottom:0;}
.recList > li:last-child .btnListMore {margin-bottom:60px;}
.recList > li:last-child .emptyBox {padding-bottom:110px;}

/* MY PAGE */
.porfile {padding:20px 15px 30px 15px; background:#fff;}
.porfile > .year {text-align:right;}
.porfile > .thumb {font-size:0; text-align:center;}
.porfile > .thumb img {display:inline-block; width:55px; border-radius:50%;}
.porfile > .name {margin-top:10px; text-align:center;}
.porfile > .name strong {font-weight:700; font-size:20px; color:#222; letter-spacing:-0.35px; vertical-align:middle;}
.porfile > .name strong a {vertical-align:middle;}
.porfile > .subDetail {text-align:center; font-size:0;}
.porfile > .subDetail span {position:relative; display:inline-block; font-size:12px; color:#6c788f; line-height:18px;}
.porfile > .subDetail span + span {padding-left:6px; margin-left:5px;}
