.menu a:first-of-type, 
.menu a:last-of-type{z-index: 3;}
.menu:before, 
.menu:after{display: block; content: ''; position: absolute; top: 0; width: 36px; height: 55px; z-index: 2;}
.menu:before{left: 0; background: -webkit-gradient(linear,  left top, right top,  from(rgba(255,255,255,1)),color-stop(50%, rgba(255,255,255,0.7)),to(rgba(255,255,255,0))); background: -webkit-linear-gradient(left,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,0) 100%); background: -o-linear-gradient(left,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,0) 100%); background: linear-gradient(to right,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,0) 100%);}
.menu:after{right: 0; background: -webkit-gradient(linear,  left top, right top,  from(rgba(255,255,255,0)),color-stop(50%, rgba(255,255,255,0.7)),to(rgba(255,255,255,1))); background: -webkit-linear-gradient(left,  rgba(255,255,255,0) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,1) 100%); background: -o-linear-gradient(left,  rgba(255,255,255,0) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,1) 100%); background: linear-gradient(to right,  rgba(255,255,255,0) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,1) 100%);}

