.card .icon i.community{background-image: url(../img/icon_community_b_55_px.svg);}
.card .icon i.experiential{background-image: url(../img/icon_experiential_b_55_px.svg);}
.card .icon i.test{background-image: url(../img/icon_test_b_55_px.svg);}
.card .icon i.mixed{background-image: url(../img/icon_mixed_b_55_px.svg);}
