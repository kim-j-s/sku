.topView .icon i.classroom{background-image: url(../img/icon_classroom_w_55_px.svg);}
.topView .icon i.learning{background-image: url(../img/icon_e_learning_w_55_px.svg);}
.topView .icon i.audio{background-image: url(../img/icon_audio_w_55_px.svg);}
.topView .icon i.video{background-image: url(../img/icon_video_w_55_px.svg);}
.topView .icon i.webpage{background-image: url(../img/icon_webpage_w_55_px.svg);}
.topView .icon i.document{background-image: url(../img/icon_document_w_55_px.svg);}
.topView .icon i.community{background-image: url(../img/icon_community_w_55_px.svg);}
.topView .icon i.experiential{background-image: url(../img/icon_experiential_w_55_px.svg);}
.topView .icon i.test{background-image: url(../img/icon_test_w_55_px.svg);}
.topView .icon i.mixed{background-image: url(../img/icon_mixed_w_55_px.svg);}
.topView .icon i.course{background-image: url(../img/icon_course_w_55_px.svg);}

/* 별점 */
.starRate{font-size: 0; line-height: 1;}
.starRate i{display: inline-block; width: 16px; height: 16px; line-height: 0; background: url(../img/icon_star_basic_32_px.svg); background-size: contain; vertical-align: top;}
.starRate i + i{margin-left: 2px;}
.starRate.s1 i:nth-child(-n+1){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s2 i:nth-child(-n+2){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s3 i:nth-child(-n+3){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s4 i:nth-child(-n+4){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s5 i:nth-child(-n+5){background-image: url(../img/icon_star_sel_32_px.svg)}

/* 상단 필터버튼 영역 */
.topGuide{display: flex; justify-content: space-between; padding: 30px 15px 0; margin: 0 0 15px;}
.topGuide .txt{font-size: 12px; line-height: 30px; color: #6c788f; letter-spacing: -0.25px}
.btnFilter{height: 30px; padding: 0 7.5px 0 6px; border: 1px solid #6c788f; border-radius: 4px; font-size: 14px; font-weight: 700; line-height: 20px; letter-spacing: -.26px; color: #6c788f; vertical-align: top;}
.btnFilter i.filter{width: 20px; height: 20px; margin-right: 3px; background-image: url(../img/icon_list_filter_40_px.svg); vertical-align: top;}
.btnFilter em{margin-left: 0.5px; font-size: 12px; font-weight: 700; letter-spacing: -.15px; vertical-align: top;}

/* sub tab menu */
.tabBox{height: 56px;}
.tabBox .menuWrap{position: relative; width: 100%; height: 56px; background-color: #fff; border-bottom: 1px solid #c8d0e0; overflow-y: hidden; z-index: 1;}
.tabBox .menuWrap.on{box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}
.menu{font-size: 0; white-space: nowrap; overflow-x: auto; padding-bottom: 20px;}
.menu > *{position: relative;}
.menu a{display: inline-block; margin: 0 15px; padding: 17px 0 15px; line-height: 20px; font-size: 14px; color: #222; border-bottom: 3px solid #fff}
.menu a .count{display: inline-block; margin-left: 3px; font-size: 12px; line-height: 1; font-weight: 700; color: #999; vertical-align: baseline;}
.menu a + a{margin-left: 20px;}
.menu a.active{font-weight: 700; border-color: #e1002a}
.menu a:first-of-type, 
.menu a:last-of-type{z-index: 3;}
.menu:before, 
.menu:after{display: block; content: ''; position: absolute; top: 0; width: 36px; height: 55px; z-index: 2;}
.menu:before{left: 0; background: linear-gradient(to right,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,0) 100%);}
.menu:after{right: 0; background: linear-gradient(to right,  rgba(255,255,255,0) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,1) 100%);}

.emptyBox{padding: 120px 30px 70px; text-align: center;}
.emptyBox i.noCont{flex: none; display: block; margin: 0 auto 14px; }
.emptyBox .txt{font-size: 14px; color: #6c788f; letter-spacing: -.35px; line-height: 1.45;}
.emptyBox .btnLink{display: inline-block; margin-top: 4px;}

.popWrap{position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 510;}
.popBox{position: absolute; top: 0; left: 0; background-color: #fff;}
.popBox .popTitle{position: relative; padding: 15px; border-bottom: 1px solid #b9b9b9}
.popBox .popTitle p{padding-right: 40px; font-size: 16px; font-weight: 700; color: #222;}
.popBox .popTitle .btnClose{position: absolute; top: 10px; right: 15px; display: block; width: 36px; height: 36px; background: url(../img/icon_close_70_px.svg); background-size: contain; text-indent: -9999px}
.popBox .popCont{}

.full.popWrap{position: fixed; min-height: 100vh;}
.full .popBox{width: 100%; min-height: 100vh;}
.full .popBox .popTitle{}
.full .popBox .popTitle + .popCont{height: calc(100vh - 55px); overflow-y: auto; padding-bottom: 83px;}

.BtmBtn.floating{position: absolute; bottom: 0; left: 0;}
.BtmBtn{width: 100%;}
.BtmBtn .stateTxt{padding: 6px 15px; font-size: 12px; line-height: 18px; color: #ff664d; font-weight: 700; text-align: center; background-color: #fff;}
.BtmBtn .stateTxt:only-child{padding: 11px 15px;}
.BtmBtn .btnGroup{display: flex;}
.BtmBtn .btnGroup > *{height: 44px; flex: 1; font-size: 14px; font-weight: 700; color: #fff; -webkit-tap-highlight-color: transparent;}
.BtmBtn .btnGroup .btnOrange{background-color: #ff664d; transition: all .1s}
.BtmBtn .btnGroup .btnBlack{background-color: #444; transition: all .1s}
.BtmBtn .btnGroup .btnOrange:active{background-color: #b2412f}
.BtmBtn .btnGroup .btnBlack:active{background-color: #222}

.accordion .viewBox{padding: 12px 15px;}
.accordion .anch{position: relative; display: block; padding: 8px 30px 8px 0; font-size: 14px; line-height: 20.5px; font-weight: 700;}
.accordion .anch strong{font-size: 14px; color: #222;}
.accordion .anch .arrow{position: absolute; top: 6px; right: 0; display: block; width: 25px; height: 25px; background-image: url(../img/icon_arrow_open_50_px.svg); background-size: contain;}
.accordion > li.open .anch .arrow{background-image: url(../img/icon_arrow_close_50_px.svg);}
.accordion > li.open .anch strong{font-weight: 700}

.filterList > li{border-bottom: 1px solid #ddd}
.filterList .line{display: flex; justify-content: space-between;}
.filterList .selectedItem{white-space: nowrap; font-size: 12px; color: #3e8ae9; overflow: hidden; text-overflow: ellipsis;}
.filterList .selectedItem span:after{display: inline-block; content: ',';}
.filterList .selectedItem span:last-of-type:after{display: none;}
.filterList .count{flex: none; display: inline-block; min-width: 30px; margin-left: 10px; color: #4c4c4c; font-size: 12px; font-weight: 700; line-height: 18px; text-align: center; background-color: #eee; border-radius: 4px;}
.filterList .sub{padding: 29px 15px; background-color: #f4f7fd; border-top: 1px solid #a8b5cd; border-bottom: 1px solid #a8b5cd; margin-bottom: -1px;}
.filterList .sub li + li{margin-top: 30px;}


.topView{position: relative; padding: 30px 25px; background-color: #45484f; color: #fff; overflow: hidden;}
.topView .ribbon{position: absolute; top: 0; right: 0; width: 65px; height: 65px; transform: rotate(45deg);}
.topView .ribbon span{position: absolute; bottom: 50%; left: 50%;	display: block; min-width: 100px; transform: translate(-50%); font-size: 10px; line-height: 24px; background-color: #6280b2; text-align: center; box-shadow: 0px 1px 5px rgba(0,0,0,0.3)}
.topView .category{margin-bottom: 10px;}
.topView .title{display: block; padding-right: 40px; font-size: 20px; line-height: 32px; font-weight: 700; letter-spacing: -.5px;}
.topView .infoArea{margin-top: 30px; font-size: 0; line-height: 24.5px; letter-spacing: -.25px;}
.topView .infoArea span{font-size: 14px; vertical-align: bottom;}
.topView .infoArea .icon{display: inline-block; margin-right: 4px; font-weight: 700}
.topView .infoArea .icon i.ico{width: 27.5px; height: 27.5px; margin-right: 3px; vertical-align: bottom;}
.topView .infoArea .icon:after{display: inline-block; content: ''; width: 1px; height: 8px; margin-left: 5px; background-color: #969dac;}
.topView .date{margin-top: 10px; font-size: 12px; color: #c8d0e0;}

.descWrap{padding: 25px 15px 30px; background-color: #fff; font-size: 12px; border-bottom: 1px solid #ebebeb;}
.timeTable{padding-bottom: 15px;}
.timeTable table{text-align: left;}
.timeTable table th{color: #6c788f;}
.timeTable table th span{position: relative;}
.timeTable table th span:before{display: inline-block; content: ''; width: 3px; height: 3px; margin: 8px 3px 0 0; border-radius: 50%; background-color: #6c788f; vertical-align: top;}
.timeTable table td{color: #222;}
.timeTable table th, 
.timeTable table td{padding: 5px 0;}

.chargeInfo{padding-top: 20px; border-top: 1px solid #eee}
.chargeInfo > p{letter-spacing: -.2px}
.chargeInfo > p > span{display: inline-block; line-height: 20px; vertical-align: top}
.chargeInfo > p > span em + em{margin-left: 5px;}
.chargeInfo > p > span em + em:before{display: inline-block; content: ''; width: 1px; height: 8px; margin-right: 5px; background-color: #c8d0e0}
.chargeInfo > p + p{margin-top: 10px;}
.chargeInfo a{color: #0e73db; text-decoration: underline; letter-spacing: 0}

.tabCont{padding: 30px 15px 70px}
.tabCont > * + *{margin-top: 30px;}

.presentText .inner{max-height: 88px; overflow: hidden;}
.presentText .inner.open{max-height: inherit;}
.presentText .inner p{font-size: 14px; line-height: 22px; color: #282e3b; letter-spacing: -.3px; }
.presentText .link{margin-top: 20px;}
.presentText .link strong{display: block; margin-bottom: 4px; font-weight: 700; color: #282e3b;}
.presentText .link strong:before{display: inline-block; content: ''; width: 3px; height: 3px; margin: 9px 3px 0 0; border-radius: 50%; background-color: #282e3b; vertical-align: top;}
.presentText .link a{display: block; font-size: 14px; line-height: 22px; color: #0e73db; word-break: break-all; text-decoration: underline;}
.presentText .moreRead{display: block; width: 100%; margin: 15px auto 0; padding: 0 20px; font-size: 12px; color: #6c788f;}
.presentText .moreRead:after{display: inline-block; content: ''; width: 0; height: 0; margin-left: 3px; border-top: 7px solid #6c788f; border-right: 5px solid transparent; border-left: 5px solid transparent;}

.downLink{display: block; padding: 12px 75px 12px 15px; background: #fff url(../img/icon_download_50_px.svg) no-repeat center right 25px; background-size: 25px auto; border-radius: 10px; box-shadow: 0px 1px 5px rgba(204,212,231,0.3);}
.downLink .text{display: block; font-size: 12px; color: #6c788f;}

.introList{background-color: #fff; border-radius: 10px; box-shadow: 0px 1px 5px rgba(204,212,231,0.8);}
.introList > li{padding-bottom: 9px;}
.introList > li + li{border-top: 1px solid #ddd}
.introList .viewBox{padding-bottom: 3px;}
.introList .sub{padding: 0 15px 18px; font-size: 14px; line-height: 22px; letter-spacing: -.3px}

.keywordWrap{font-size: 0; margin-bottom: -10px;}
.keywordWrap > span{display: inline-block; padding: 0 11px; margin: 0 5px 10px 0; font-size: 14px; line-height: 23px; color: #6c788f; background-color: #fff; border: 1px solid #d1d6e2; border-radius: 13px;}