
/* ----------- yjm ----------- */


/*common*/
body{background: #f4f7fd;}
i.ico{display: inline-block; background-size: contain;}
i.noCont{width: 60px; height: 60px; background-image: url(../img/img_no_contents_80_px.svg);}
i.link{width: 10px; height: 10px; background-image: url(../img/icon_contents_link_20_px.svg);}
.btnLink{color: #0e73db; font-size: 12px; letter-spacing: -.35px}
.btnLink span{text-decoration: underline;}
.btnLink i.link{margin-left: 2px; vertical-align: middle;}

/* checkbox */
.inpD label{position: relative; padding-left: 24px;}
.inpD label input{position: absolute; top: 0; left: 0; opacity: 0; z-index: -1; outline: 0;}
.inpD label .txt{display: inline-block; margin: 1.5px 0 0 10px; font-size: 14px; line-height: 20px; color: #222; vertical-align: top;}
.inpD label i.checkBox{position: absolute; top: 0; left: 0; width: 24px; height: 24px; background-image: url(../img/btn_check_nomal.svg);}
.inpD label input[type="checkbox"]:checked + .checkBox{background-image: url(../img/btn_check_selected.svg);}

/* button */
.btnListMore{display: block; margin: 30px auto 60px; padding: 0 10px; font-size: 12px; font-weight: 700; color: #6c788f; line-height: 18px;}
.btnListMore em{font-weight: 700}
.btnListMore i.plus{display: inline-block; width: 13px; height: 13px; margin: 2.5px 3px 0 0; background: url(../img/icon_list_more.svg); background-size: contain; vertical-align: top;}

/* card */
.card{display: block; margin: 0 15px 15px; padding: 24px 20px; background-color: #fff; box-shadow: 0px 1px 5px rgba(204,212,231,.8); border-radius: 10px}
.card .cardInner{position: relative;}
.card .titleArea{margin-bottom: 10px;}
.card .infoArea{width: 100%; font-size: 14px; line-height: 1; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.card .btmArea{display: flex; justify-content: space-between; align-items: center; margin: 20px -20px -24px; padding: 12px 20px 15px; border-top: 1px solid #eee; font-size: 12px; color: #666;}
.card .icon{position: absolute; top: -4px; right: 0; font-size: 10px; font-weight: 700; color: #222; line-height: 27.5px;}
.card .icon i{width: 27.5px; height: 27.5px; margin-left: 5px; vertical-align: top;}
.card .category{margin-bottom: 25px;}
.card .title{display: -webkit-box; height: 54px; padding-right: 20px; font-size: 18px; color: #222; font-weight: 700; line-height: 27px; letter-spacing: -.45px; 	overflow: hidden; text-overflow: ellipsis; 	-webkit-line-clamp: 2; -webkit-box-orient: vertical; word-wrap:break-word;}
.card .label{display: inline;}
.card .label i{width: 16px; height: 16px; margin: 0 2px 0 0; vertical-align: top;}
.card .label b{color: #6c788f; font-size: 14px; vertical-align: top;}
.card .label + .label{margin-left: 6px;}
.card .tag{padding-top: 3px; color: #6280b2;}
.card .tag span{line-height: 1.45;}
.card .tag + .date{margin-top: 2px;}
.card .date{font-size: 12px; line-height: 1.5; color: #666; letter-spacing: 0.5px}
.card .state{color: #222;}

/* course card */
.cardWrap{margin: 0 15px; background-color: #fff; box-shadow: 0px 1px 5px rgba(204,212,231,.8); border-radius: 10px; overflow: hidden;}
.cardWrap .card{display: block; margin: 0; border-radius: 0; box-shadow: none;}
.cardWrap .card + .card{border-top: 1px solid #eee}
.cardWrap .card .title{font-weight: 400}
.cardWrap .card.course ~ .card{display: none; padding-top: 28.5px;}
.cardWrap .card.course .category{margin-bottom: 29.5px;}
.cardWrap .card.course .title{font-weight: 700}
.card.course .infoArea{padding-right: 30px;}
.cardWrap .cardToggle{position: absolute; right: 0; bottom: 0; width: 30px; height: 30px; background: url(../img/btn_card_close.svg); background-size: contain; transform: rotate(180deg);}
.cardWrap.open .cardToggle{transform: rotate(0);}

.card .label i.time{background-image: url(../img/icon_time_card_32_px.svg);}
.card .label i.stamp{background-image: url(../img/icon_list_stamp_32_px.svg);}
.card .label i.map{background-image: url(../img/icon_card_map_32_px.svg);}
.card i.check{width: 10px; height: 10px; margin-right: 3px; background-image: url(../img/icon_check_16_px.svg);}

/* card college tag color */
.category{display: inline-block; min-width: 30px; padding: 1.5px 6px 2.5px; font-size: 10px; line-height: 15px; font-weight: 700; color: #fff; letter-spacing: -.08px; border-radius: 2px; vertical-align: top; text-align: center;}
.category.red{background-color:#db2828;}
.category.orange{background-color:#f2711c;}
.category.yellow{background-color:#fbbd08;}
.category.teal{background-color:#00b5ad;}
.category.green{background-color:#21ba45;}
.category.blue{background-color:#2185d0;}
.category.violet{background-color:#6435c9;}
.category.purple{background-color:#a333c8;}

/* card icon img*/
.card .icon i.classroom{background-image: url(../img/icon_classroom_b_55_px.svg);}
.card .icon i.learning{background-image: url(../img/icon_e_learning_b_55_px.svg);}
