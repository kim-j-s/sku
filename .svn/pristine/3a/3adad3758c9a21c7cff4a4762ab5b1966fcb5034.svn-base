

/* common element */
/* input */
.inputBox {position:relative; display:inline-block;}
.inputBox.full {width:100% !important;}
.inputBox .inp {display:block; width:100%; height:35px; padding:0 15px; font-size:14px; border:1px solid #f4f4f4; transition:0.3s; background:transparent;}
.inputBox .inp:disabled {}
.inputBox .inp:focus {border:1px solid #569df6;}
.inputBox.sch {position:static; border-radius:3px;}
.inputBox.sch .inp {padding-right:40px; border:1px solid #f4f4f4; border-radius:3px; transition:0.3s;}
.inputBox.sch .inp.on {padding-right:70px;}
.inputBox.sch .inp {background:transparent;}
.inputBox.sch .inp:focus {background:#f4f4f4; border:1px solid #569df6;}
.inputBox.sch .schBtn {position:absolute; top:5px; right:10px;}
.inputBox.sch.on {z-index:2;}
.inputBox.sch.on .inp {background:#f4f4f4;}
.inputBox.sch .inpReset {position:absolute; top:5px; right:40px; opacity:0; transition:0.3s;}

.inputBox.date {}
.inputBox.date .inp {padding-left:46px; line-height:33px; border:1px solid #6c788f; border-radius:4px; background:#fff url(../img/icon_date_40_px.svg) left 7px center no-repeat; background-size:20px auto;}
.inputBox.date:before {content:''; position:absolute; top:0; left:35px; height:100%; border-left:1px solid #6c788f;}
.inputBox.date .dateLabel {position:absolute; top:2px; left:2px; display:block; width:calc(100% - 39px); height:calc(100% - 4px); padding-left:35px; overflow:hidden; text-indent:-9999px; text-align:left; border-radius:4px;}
.inputBox.date .dateLabel > span {display:block; height:100%; background:rgba(0,0,0,0.2); background:#fff;}

/* input checkbox switch */
.switch {display:inline-block; font-size:0; width:44px; height:24px; border-radius:12px;}
.switchCheckbox {position:relative; width:44px; height:24px; border-radius:12px; outline:none; background:#d9d9d9; transition:all .2s ease-in-out;}
.switchCheckbox {-webkit-appearance: none; -moz-appearance: none; appearance: none;}
.switchCheckbox:checked {background:#3e8ae9;}
.switchCheckbox:after {content:''; position:absolute; top:50%; left:3px; width:18px; height:18px; margin-top:-9px; border-radius:50%; background:#fff; box-shadow: 0px 1px 4px 1px rgba(0,0,0,0.2); transition: all .2s ease-in-out;}
.switchCheckbox:checked:after {left:calc(100% - 21px);}

/* button */
.btn {display:inline-block; font-size:0;}
.btn.schBtn {width:25px; height:25px; background:url(../img/icon_gnb_search_50_px.svg) left top no-repeat; background-size:100%;}
.btn.close {position:relative; width:24px; height:24px; padding:5px; overflow:hidden;}
.btn.close:before {content:''; position:absolute; top:11px; left:3px; width:18px; border-bottom:2px solid #000; transform: rotate(45deg);}
.btn.close:after {content:''; position:absolute; top:11px; right:3px; width:18px; border-bottom:2px solid #000; transform: rotate(135deg);}
.btn.close2 {width:35px; height:35px; overflow:hidden; text-indent:-9999px; background:url(../img/icon_close_w_70_px.svg) left top no-repeat; background-size:contain;}
.btn.inpReset {width:25px; height:25px; background:url(../img/btn_input_del_selected.svg) left top no-repeat; background-size:contain;}
.btn.inpReset.on {opacity:1;}
.btn.scMenuBtn {width:30px; height:30px; overflow:hidden; text-indent:-9999px; background:url(../img/btn_title_shot_white_60_px.svg) left top no-repeat; background-size:30px 30px; /* transition:0.3s; */}
.btn.pageBack {width:30px; height:30px; overflow:hidden; text-indent:-9999px; background:url(../img/btn_title_arrow_white_60_px.svg) left top no-repeat; background-size:30px 30px; /* transition:0.3s; */}
.btn.scBtn {position:relative; display:block; height:18px; line-height:18px; padding-left:26px; font-weight:700; font-size:12px; color:#222;}
.btn.scBtn:before {content:''; position:absolute; top:1px; left:0; width:16px; height:16px;}
.btn.scBtn.ty1:before {background:url(../img/icon_add_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty2:before {background:url(../img/icon_delete_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty3:before {background:url(../img/icon_share_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty4:before {background:url(../img/icon_survey_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty5:before {background:url(../img/icon_download_32_px.svg) left top no-repeat; background-size:contain;}
.btn.scBtn.ty6:before {background:url(../img/icon_complete_32_px.svg) left top no-repeat; background-size:contain;}
.btn.gototop {width:34px; height:34px; border:1px solid #6280b2; border-radius:4px; background:#fff; box-shadow: 0px 0px 3px 0px rgba(189,199,224,1);}
.btn.gototop > span {position:relative; display:block; padding-top:14px; font-weight:700; font-size:10px; color:#222; line-height:14px; text-align:center;}
.btn.gototop > span:before {content:''; position:absolute; top:0; left:50%; margin-left:-7px; width:14px; height:14px; background:url(../img/icon_top_28_px.svg) center center no-repeat; background-size:contain;}
.btn.channelBtn {display:inline-block; margin:0 4px 10px 4px; padding:1px 12px; line-height:21px; font-size:14px; color:#233b6a; border:1px solid #3e8ae9; border-radius:12px;}
.btn.channelBtn.on {color:#fff; background:#3e8ae9;}
.btn.sort {text-align:center;}
.btn.sort > span {display:inline-block; vertical-align:middle; padding-left:3px; font-weight:700; font-size:14px; color:#6c788f;}
.btn.sort.ty1:before {content:''; display:inline-block; width:20px; height:20px; background:url(../img/icon_list_filter_40_px.svg) left top no-repeat; background-size:contain; vertical-align:middle;}
.btn.sort.ty2:before {content:''; display:inline-block; width:20px; height:20px; background:url(../img/icon_list_solting_40_px.svg) left top no-repeat; background-size:contain; vertical-align:middle;}
.btn.closeSmall {width:14px; height:14px; overflow:hidden; text-indent:-9999px; background:url(../img/icon_close_b_28_px.svg) left top no-repeat; background-size:contain;}
.btn.dateReset {width:25px; height:25px; background:url(../img/icon_refresh_50_px.svg) left top no-repeat; background-size:contain;}
.btn.channelLink {display:block; padding:12px 0; text-align:center; border:1px solid #6c788f; border-radius:6px; background:#fff;}
.btn.channelLink > span {position:relative; display:inline-block; padding-right:16px; font-weight:700; font-size:14px; color:#6c788f; line-height:20px; background:url(../img/icon_arrow_blue_2_28_px.svg) right center no-repeat; background-size:14px auto;}

/* selectbox */
.selectbox {display:inline-block; height:30px; padding-right:20px; border:1px solid #fff; border-radius:0;}
.selectbox {background:url(../img/icon_arrow_open_28_px.svg) right center no-repeat; background-size:14px 14px;}

/* ----------- kjs ----------- */
header {position:relative; z-index:6; height:55px;}
header h1 {position:absolute; top:0; left:0; width:100%; height:100%; z-index:-1; border-radius:3px; overflow:hidden; text-indent:-9999px; background:#f4f4f4;}
header h1:before {content:''; position:absolute; top:12px; left:15px; width:33px; height:12px; background:url(../img/logo.svg) center center no-repeat; background-size:contain;}
header h1.on:before {opacity:0; transition:1s;}

header .keyWord {position:absolute; top:0; left:0; width:100%; height:100%; z-index:-2; padding:9px 40px 8px 15px; border-radius:3px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; font-size:12px; color:#666; line-height:18px; background:#f4f4f4;}
header .keyWord > span {color:#e1002a;}
header .keyWord.on {z-index:-1;}

/* header Type Main */
.header {position:fixed; top:0; left:0; z-index:1; width:100%; padding:10px 15px 10px 53px; border-bottom:1px solid #c8d0e0; background:#fff; transition:0.3s;}
.header.on {box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}
.header > .inner {position:relative;}

/* detail header */
.detailHeader {position:fixed; top:0; left:0; z-index:1; width:100%; height:55px; padding:12px 50px 12px 45px; border-bottom:1px solid #646872; background:#45484f; /* transition:0.3s; */}
.detailHeader .btn.pageBack {position:absolute; top:12px; left:15px;}
.detailHeader .btn.scMenuBtn {}
.detailHeader .keyWord {position:static; padding:0 5px 0 0; color:#fff; line-height:30px; background:#45484f; /* transition:0.3s; */}
.detailHeader.on {border-bottom:1px solid #c8d0e0; box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2); background:#fff;}
.detailHeader.on.none {box-shadow:none;}
.detailHeader.on .btn.scMenuBtn {background:url(../img/btn_title_shot_black_60_px.svg) left top no-repeat; background-size:contain;}
.detailHeader.on .btn.pageBack {background:url(../img/btn_title_arrow_black_60_px.svg) left top no-repeat; background-size:contain;}
.detailHeader.on .keyWord {position:static; padding:0 5px 0 0; color:#222; line-height:30px; background:#fff;}

/* list header */
.listHeader {position:fixed; top:0; left:0; z-index:1; width:100%; height:55px; padding:12px 15px 12px 45px; border-bottom:1px solid #c8d0e0; background:#fff; transition:0.3s;}
.listHeader .btn.pageBack {position:absolute; top:12px; left:15px; background:url(../img/btn_title_arrow_black_60_px.svg) left top no-repeat; background-size:contain;}
.listHeader .keyWord {position:static; padding:0; font-weight:700; font-size:16px; color:#222; line-height:30px; background:#fff;}
.listHeader.on {box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}

/* gnb */
.gnbOpen {position:absolute; top:10px; left:15px; width:35px; height:35px; overflow:hidden; text-indent:-9999px; background:url(../img/icon_menu_nomal_70_px.svg) left top no-repeat; background-size:contain;}
.gnbOpen.new {background:url(../img/icon_menu_new_70_px.svg) left top no-repeat; background-size:contain;}
nav {position:fixed; top:0; right:0; bottom:0; left:-100%; width:100%; z-index:101; overflow:auto; background:#fff; transition:0.3s;}
nav.on {left:0;}
.navTop {position:relative; z-index:1; height:55px; background:#2d2d2d;}
.navTop:before {content:''; position:absolute; top:21px; left:20px; width:40px; height:14px; background:url(../img/icon_menu_logo.svg) center center no-repeat; background-size:contain;}
.navTop .btn.close2 {position:absolute; top:10px; right:20px;}

.gnb {position:absolute; top:0; left:0; padding-top:55px; overflow:auto; width:100%; height:100%;}
.gnbList {padding-bottom:50px;}
.gnbList > li {position:relative; padding:15px 20px 15px 45px; border-bottom:1px solid #c8d0e0;}
.gnbList > li:before {content:''; position:absolute; top:15px; left:20px; width:20px; height:20px; background-position:center center; background-size:contain;}
.gnbList > li.ty1:before {background-image:url(../img/icon_menu_home_40_px.svg);}
.gnbList > li.ty2:before {background-image:url(../img/icon_menu_learning_40_px.svg);}
.gnbList > li.ty3:before {background-image:url(../img/icon_menu_recommend_40_px.svg);}
.gnbList > li.ty4:before {background-image:url(../img/icon_menu_comm_40_px.svg);}
.gnbList > li.ty5:before {background-image:url(../img/icon_menu_support_40_px.svg);}
.gnbList > li.ty6:before {background-image:url(../img/icon_menu_search_40_px.svg);}
.gnbList > li.ty7:before {background-image:url(../img/icon_menu_my_40_px.svg);}
.gnbList > li.ty8:before {background-image:url(../img/icon_menu_channel_40_px.svg);}
.gnbList > li.ty9:before {background-image:url(../img/help.svg);}
.gnbList > li > span {display:block; line-height:20px; font-weight:700; font-size:16px; color:#222;}
.gnbList > li > a {position:relative; display:block; line-height:20px; font-weight:700; font-size:16px; color:#222;}
.gnbList > li > a:after {content:''; position:absolute; top:-2px; right:0; width:25px; height:25px; background:url(../img/icon_arrow_go_50_px.svg) center center no-repeat; background-size:contain;}
.gnbList > li.ty9 > a:after {content:none;}
.gnbList > li > a small {font-size:12px; color:#999;}
.gnbList > li > a.tel {position:absolute; top:17px; right:20px; font-weight:normal; font-size:12px; color:#0e73db; text-decoration:underline; line-height:18px;}

.gnbListDept {padding:10px 0 5px 0;}
.gnbListDept > li {}
.gnbListDept > li + li {margin-top:10px;}
.gnbListDept > li a {display:block; font-size:14px; color:#222;}
.gnbListDept > li a span {font-size:12px; color:#999;}

/* content */
.content {}

/* shortCut */
.shortCuts {position:absolute; top:12px; right:20px; z-index:1;}
.shortCutList {position:absolute; top:30px; right:-5px; display:none; width:180px; border-radius:8px; background:#fff; box-shadow: 0px 4px 8px 0px rgba(34,34,34,0.53);}
.shortCutList > li {padding:13px 0 13px 15px;}
.shortCutList > li + li {border-top:1px solid #ddd;}

/* 공지사항 */
.noticeWrap {position:relative; padding:16px 56px 16px 15px; overflow:hidden; background:#fff;}
.noticeWrap.on {height:0; padding:0 40px 0 15px; border:none; transition:.5s;}
.noticeWrap > a {position:relative; display:block; padding-left:27px; line-height:16px; font-size:12px; color:#666; overflow: hidden; text-overflow: ellipsis; white-space: nowrap;}
.noticeWrap > a:before {content:''; position:absolute; top:0; left:0; width:16px; height:16px; background:url(../img/notice.svg) center center no-repeat; background-size:100%;}
.noticeWrap > .btn.close {position:absolute; top:13px; right:16px;}

/* Toolbar */
.toolBar {position:fixed; left:0; bottom:0; z-index:5; width:100%; height:55px; background:rgba(45,45,45,0.95);}
.toolList {display:flex;}
.toolList > li {flex:1; padding-top:10px;}
.toolList > li > a {position:relative; display:block; padding-top:21px; text-align:center; font-size:10px; color:#eee; line-height:14px;}
.toolList > li > a:before {content:''; position:absolute; top:0; left:50%; margin-left:-10px; width:20px; height:20px;}
.toolList > li.new > a:after {content:''; position:absolute; top:0; left:50%; margin-left:5px; width:10px; height:10px; background:url(../img/new_badge.svg) center center no-repeat; background-size:contain;}
.toolList > li.on > a  {font-weight:700;}
.toolList > li.ty1 > a:before {background:url(../img/icon_home_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty1.on > a:before {background:url(../img/icon_home_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty2 > a:before {background:url(../img/icon_learning_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty2.on > a:before {background:url(../img/icon_learning_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty3 > a:before {background:url(../img/icon_comm_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty3.on > a:before {background:url(../img/icon_comm_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty4 > a:before {background:url(../img/icon_notice_normal_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty4.on > a:before {background:url(../img/icon_notice_sel_40_px.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty5 > a:before {background:url(../img/icon_40_px_bottom_my_normal.svg) center center no-repeat; background-size:contain;}
.toolList > li.ty5.on > a:before {background:url(../img/icon_my_select_40_px.svg) center center no-repeat; background-size:contain;}

/* page top */
.pageTop {position:fixed; bottom:75px; right:10px; z-index:1; opacity:0; transition:0.3s;}
.pageTop.on {opacity:1;}

/* 메인 - 추천 과정 */
.recCourseWrap {position:relative; padding-top:30px; background:#6280b2;}
.recCourseWrap > .tit {padding:0 0 15px 15px; font-size:16px; color:#fff; letter-spacing:-0.35px; line-height:24px;}
.recCourseWrap > .tit strong {font-weight:700;}

.recCourse {overflow:hidden;}
.recCourse .swiper-slide {padding:0 15px 60px 15px;}
.recCourse.swiper-container-initialized .swiper-slide {padding:0 15px 20px 15px;}
.recCourse .swiper-slide .txt {padding:0 0 12px 0; font-size:0;}
.recCourse .swiper-slide .txt a {position:relative; display:inline-block; padding-right:17px; font-size:12px; color:#fff; line-height:18px; letter-spacing:-0.2px;}
.recCourse .swiper-slide .txt a:after {content:''; position:absolute; top:2px; right:0; width:14px; height:14px; background:url(../img/icon_arrow_white_28_px.svg) center center no-repeat; background-size:contain;}
.recCourse .swiper-slide .txt strong {font-weight:700;}
.recCourse .card {margin:0; box-shadow: 0px 1px 5px 0px rgba(72,106,162,1);}
.recCourse .swiper-pagination {padding-bottom:25px;}

/* 메인 - 추천 과정 목록 */
.mainRec {}
.mainRec > .inner {padding:0 15px;}
.mainRec > .inner > .btn.channelLink {margin-top:40px;}
.mainRec > .inner + .recList .recTit {padding-top:40px;}
.recList {}
.recList > li {}
.recList > li + li {margin-top:10px;}
.recList > li .recTit {padding:30px 15px 15px 15px;}
.recList > li .card:first-child {margin-top:30px;}
.recList > li .emptyBox {padding:60px 15px 50px 15px;}
.recList > li .emptyBox.noSch {padding:90px 15px 50px 15px;}
.recList > li .btnListMore {margin-bottom:0;}
.recList > li .recTit + .emptyBox {padding-top:50px;}
.recList > li .recTit .switch + .sTxt {display:inline-block; padding-left:10px; vertical-align:middle; font-size:12px; color:#6c788f; letter-spacing:-0.2px;}
.recList > li .recTit + .emptyBox.noSch {padding-top:20px;}
.recList > li:last-child .btnListMore {margin-bottom:60px;}
.recList > li:last-child .emptyBox {padding-bottom:110px;}

.recTit {font-weight:700; padding:30px 15px 15px 15px; font-size:0; color:#222; line-height:20px; letter-spacing:-0.3px;}
.recTit * {vertical-align:top;}
.recTit a {position:relative; display:inline-block; font-weight:700;}
.recTit a * {vertical-align:top;}
.recTit strong {display:inline-block; font-weight:700; font-size:14px; color:#4072c6;}
.recTit .cName {display:inline-block; padding-right:3px; font-weight:700; font-size:14px; color:#4072c6;}
.recTit .txt {position:relative; display:inline-block; font-size:14px;}
.recTit .txtEnd {position:relative; display:inline-block; padding-right:16px;}
.recTit .txtEnd:after {content:''; position:absolute; bottom:3px; right:0; display:inline-block; width:14px; height:14px; background:url(../img/icon_arrow_black_28_px.svg) center center no-repeat; background-size:contain;}
.recTit .txt small {font-size:12px;}

/* MY PAGE */
.porfile {padding:20px 15px 30px 15px; background:#fff;}
.porfile > .year {text-align:right;}
.porfile > .thumb {font-size:0; text-align:center;}
.porfile > .thumb img {display:inline-block; width:55px; border-radius:50%;}
.porfile > .name {margin-top:10px; text-align:center;}
.porfile > .name strong {font-weight:700; font-size:20px; color:#222; letter-spacing:-0.35px; vertical-align:middle;}
.porfile > .name strong a {vertical-align:middle;}
.porfile > .subDetail {text-align:center; font-size:0;}
.porfile > .subDetail span {position:relative; display:inline-block; font-size:12px; color:#6c788f; line-height:18px;}
.porfile > .subDetail span + span {padding-left:6px; margin-left:5px;}
.porfile > .subDetail span + span:before {content:''; position:absolute; top:50%; left:0; height:8px; margin-top:-4px; border-left:1px solid #c8d0e0;}

.learnTotal {border-top:1px solid #c8d0e0;}
.learnTotal > .inner {margin-top:5px; padding:22px 15px 30px 15px; background:#fff;}
.ltTit {display:table; width:100%; table-layout:fixed;}
.ltTit > dt, .ltTit > dd {display:table-cell; vertical-align:middle;}
.ltTit > dt {font-size:14px; color:#222; letter-spacing:-0.2px; text-align:left;}
.ltTit > dt > a {color:#222; text-decoration:underline;}
.ltTit > dt > strong {font-weight:700;}
.ltTit > dt > strong + small {display:block; padding-top:1px; font-size:12px; color:#666; letter-spacing:-0.25px;}
.ltTit > dd {font-weight:700; font-size:16px; color:#222; text-align:right;}
.ltTit > dd span {display:block; font-weight:400; font-size:14px; color:#6c788f;}
.ltTit > dd strong {font-weight:700; padding-right:1px;}
.ltTit + .recView {margin-top:20px; font-size:0;}

.learnTotal.big {border-top:none;}
.learnTotal.big > .inner {margin-top:0; padding:24px 15px 30px 15px;}
.learnTotal.big .ltTit > dd {font-size:24px;}
.learnTotal.big #chart2 {display:inline-block !important; width:117px; height:117px;}

.chartWrap {margin-top:20px; text-align:center; font-size:0;}
#chart {display:inline-block !important; width:78px; height:78px;}

.learnListWrap {margin-top:15px; text-align:center;}
.learnList {display:inline-block;}
.learnList > li {position:relative; padding-left:8px; font-size:0;}
.learnList > li:before {content:''; position:absolute; top:50%; left:0; margin-top:-1px; width:3px; border-radius:50%;}
.learnList > li.ty1:before {border-bottom:3px solid #ff664d;}
.learnList > li.ty2:before {border-bottom:3px solid #545d8b;}
.learnList > li + li {margin-top:4px;}
.learnList > li > span {display:inline-block; font-size:14px; color:#666;}
.learnList > li .tit {width:97px; text-align:left;}
.learnList > li .time {width:66px; color:#4c4c4c; text-align:right;}

.recView {text-align:center; padding:24px 0; border-radius:4px; background:#f4f7fd;}
.recView > .btnLink {font-weight:700;}
.recView > .btnLink span, .recView > .btnLink i.ico {vertical-align:middle;}

.linkListWrap {padding-top:5px; padding-bottom:60px; border-top:1px solid #c8d0e0;}
.linkList {background:#fff;}
.linkList > li {position:relative; padding:17px 15px; border-bottom:1px solid #ddd; line-height:25px; font-size:14px; color:#222; letter-spacing:-0.3px;}
.linkList > li + li {}
.linkList > li a {position:relative; display:block; padding-right:25px;}
.linkList > li a:after {content:''; position:absolute; top:0; right:0; width:25px; height:25px; background:url(../img/icon_arrow_go_50_px.svg) center center no-repeat; background-size:contain;}
.linkList > li a strong {position:absolute; top:0; right:25px; text-align:right; font-weight:700;}
.linkList > li .switch {position:absolute; top:17px; right:15px;}

/* College 별 학습 시간 */
.learnTotal + .learnTime {border-top:1px solid #c8d0e0;}
.learnTime {padding:30px 15px 60px 15px; background:#f4f7fd;}
.learnTime > .tit {padding-bottom:15px; font-weight:700; font-size:14px; color:#4072c6; letter-spacing:-0.35px;}
.ltList {padding:30px 25px; border-radius:10px; background:#fff; box-shadow: 0px 1px 5px 0px rgba(204,212,231,1);}
.ltList > li {position:relative; padding-left:10px;}
.ltList > li + li {margin-top:20px;}
.ltList > li:before {content:''; position:absolute; top:50%; left:0; width:5px; margin-top:-2px; border-radius:50%;}
.ltList > li.ty1:before {border-bottom:5px solid #9f3fc5;}
.ltList > li.ty2:before {border-bottom:5px solid #613fc6;}
.ltList > li.ty3:before {border-bottom:5px solid #ffb306;}
.ltList > li.ty4:before {border-bottom:5px solid #ee712c;}
.ltList > li.ty5:before {border-bottom:5px solid #d72d2f;}
.ltList > li.ty6:before {border-bottom:5px solid #38b84c;}
.ltList > li.ty7:before {border-bottom:5px solid #2987cd;}
.ltList > li.ty8:before {border-bottom:5px solid #28b4ac;}
.ltClass {display:table; width:100%; table-layout:fixed;}
.ltClass > dt, .ltClass > dd {display:table-cell; vertical-align:middle;}
.ltClass > dt {font-weight:700; font-size:14px; color:#222; letter-spacing:-0.35px; line-height:20px;}
.ltClass > dd {text-align:right; font-size:14px; color:#222;}

/* Recommend */
.recViewsLink {display:block;}
.recViewsLink + .recList {padding-bottom:0; border-top:1px solid #e3e7ef;}
.recViewsLink + .recList > li:last-child .emptyBox {padding-bottom:110px;}
.recViews {position:relative; padding:30px 0 10px 0; color:#222; background:#fff; border-bottom:1px solid #e3e7ef;}
.recViews > .tit {padding:0 85px 0 15px; font-size:0; letter-spacing:-0.3px;}
.recViews > .tit.etc {padding:0 15px;}
.recViews > .tit.etc2 {display:block; padding:0 15px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; font-size:14px;}
.recViews > .tit > span {display:inline-block; vertical-align:middle;}
.recViews > .tit.etc2 > span {display:inline; vertical-align:top;}
.recViews > .tit .cName {position:relative; padding-right:5px; font-weight:700; font-size:14px; color:#4072c6;}
.recViews > .tit.etc2 .cName {padding-right:2px;}
.recViews > .tit .cName.etc {padding-right:0;}
.recViews > .tit .cName:before {content:'‘';}
.recViews > .tit .cName:after {content:'’';}
.recViews > .tit .txt {font-weight:700; font-size:14px;}
.recViews > .tit .txt2 {font-weight:700; font-size:14px;}
.recViews > .tit .cnt {padding-left:2px; font-weight:700; font-size:12px;}
.recViews > .btnLink {position:absolute; top:30px; right:15px;}
.recViews > .btnLink > * {vertical-align:middle;}
.recViews > .btnLink > i.ico {vertical-align:middle;}
.recViews > .tit + .sortChannel {margin-top:30px; padding:0 15px 20px 15px;}

.channels {padding:20px 11px 10px 11px; font-size:0;}
.channelName {display:inline-block; margin:0 4px 10px 4px; padding:2px 12px; line-height:21px; font-size:14px; color:#fff; border-radius:12px; background:#3e8ae9;}

.optList {}
.optList > li {padding:18px 15px; border-bottom:1px solid #eee;}
.optList > li .inpD label input[type="radio"] ~ .txt {color:#222;}

.sortChannel {}
.sortChannel > .inner {display:table; width:100%; font-size:0; border:1px solid #6c788f; border-radius:4px; table-layout:fixed;}
.sorting {display:table-cell; vertical-align:top;}
.sorting + .sorting {border-left:1px solid #6c788f;}
.sorting > .btn {display:block; width:100%; height:28px; vertical-align:top;}
.sorting > .btnFilter {display:block; width:100%; height:28px; border:none; padding:0;}

.filterItem {height:41px; overflow-y:hidden;}
.filterItem > .inner {padding-left:15px; padding-right:15px; padding-bottom:20px; font-size:0; white-space:nowrap; overflow-x:auto;}
.flName {display:inline-block; margin-top:15px; padding:2px 6px 2px 12px; vertical-align:top; border:1px solid #d1d6e2; border-radius:12px;}
.flName + .flName {margin-left:5px;}
.flName > .txt {display:inline-block; font-size:14px; color:#6c788f; vertical-align:top; line-height:20px;}
.flName > .txt + .btn {position:relative; top:4px; margin-left:5px; vertical-align:top;}
.filterItem + .sortChannel {padding:30px 15px 20px 15px;}

.datePick {}
.datePick > .tit {font-size:14px; color:#222;}
.datePickers {position:relative; display:flex; margin-top:12px;}
.datePickers > .ele {position:relative; text-align:center; flex:1; width:calc(50% - 10px);}
.datePickers > .ele + .ele {padding-left:20px;}
.datePickers > .ele + .ele:before {content:'-'; position:absolute; top:0; left:0; width:20px; line-height:35px; text-align:center; font-size:12px; color:#6c788f;}
.datePickers > .ele .inputBox {display:block;}
.datePickers .dateReset {position:absolute; right:0; top:-35px;}

.collegeList {padding:0 15px 60px 15px; font-size:0;}
.collegeList > li {display:inline-block; width:50%; margin-top:15px; padding-right:5px; vertical-align:top;}
.collegeList > li:nth-child(even) {padding:0 0 0 5px;}
.collegeList > li:nth-child(-n + 2) {margin-top:0px;}

.collegeItem {display:block; height:160px; padding:20px; border-radius:10px; box-sizing:border-box; background:#fff; box-shadow:0px 1px 5px 0px rgba(204,212,231,0.8);}
.collegeTit {position:relative; display:block; padding-top:59px; font-weight:700; font-size:14px; color:#222;}
.collegeTit:before {content:''; position:absolute; top:0; left:0; width:50px; height:50px; background-size:contain;}
.collegeTit.ty1:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty2:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty3:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty4:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty5:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty6:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty7:before {background-image:url(../img/img_colleage_ai.jpg);}
.collegeTit.ty8:before {background-image:url(../img/img_colleage_ai.jpg);}


.collegeTit > span {font-size:12px; color:#999; vertical-align:baseline;}
.collegeItem > .txt {padding-top:5px; font-size:12px; color:#6c788f; line-height:18px; letter-spacing:-0.2px; white-space:nowrap;}







