    <!-- // .header -->

    <div class="noticeWrap">
        <a href="#">UX DESIGN강좌 11월 수강신청 안내</a>
        <button class="btn close">닫기</button>
    </div>

    <!-- .content -->
    <section class="content">
        <div class="con">content</div>
        <style>
            .con {
                height: 1500px;
                background: #ddd;
            }
        </style>
    </section>
    <!-- // .content -->
