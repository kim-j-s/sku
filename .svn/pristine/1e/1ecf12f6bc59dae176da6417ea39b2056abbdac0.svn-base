                    <span class="inpD num">
						<label>
							<input type="radio" name="radio1">
							<span class="txt">1</span>
                    <i class="ico radio"></i>
                    </label>
                    </span>
                    <span class="inpD num">
						<label>
							<input type="radio" name="radio1" checked="checked">
							<span class="txt">2</span>
                    <i class="ico radio"></i>
                    </label>
                    </span>
                    <span class="inpD num">
						<label>
							<input type="radio" name="radio2" checked="checked" disabled="disabled">
							<span class="txt">3</span>
                    <i class="ico radio"></i>
                    </label>
                    </span>
                </div>

                <p class="gInfo">채널 버튼 선택</p>
                <div class="preview">
                    <div class="">
                        <button type="button" class="btn channelBtn on">btn1</button>
                        <button type="button" class="btn channelBtn">btn2</button>
                    </div>
