                <h2 class="test">Checkbox / Radio</h2>

                <p class="gInfo">checkbox</p>
                <div class="preview">
                    <div>
                        <span class="inpD">
							<label>
								<input type="checkbox">
								<span class="txt">default</span>
                        <i class="ico checkBox"></i>
                        </label>
                        </span>
                    </div>
                    <div>
                        <span class="inpD">
							<label>
								<input type="checkbox" checked="checked">
								<span class="txt">checked</span>
                        <i class="ico checkBox"></i>
                        </label>
                        </span>
                    </div>
                    <div>
                        <span class="inpD">
							<label>
								<input type="checkbox" checked="checked" disabled="disabled">
								<span class="txt">checked + disabled</span>
                        <i class="ico checkBox"></i>
                        </label>
                        </span>
                    </div>
                </div>

                <p class="gInfo">checkbox (.black)</p>
                <div class="preview">
                    <div>
                        <span class="inpD black">
							<label>
								<input type="checkbox">
								<span class="txt">checked</span>
                        <i class="ico checkBox"></i>
                        </label>
                        </span>
                    </div>
                    <div>
                        <span class="inpD black">
							<label>
								<input type="checkbox" checked="">
								<span class="txt">checked + disabled</span>
                        <i class="ico checkBox"></i>
                        </label>
                        </span>
                    </div>
                </div>

                <p class="gInfo">스위치</p>
                <div class="preview">
                    <div class="switch">
                        <input type="checkbox" name="" id="switch" class="switchCheckbox" checked="checked">
                    </div>
                </div>

                <p class="gInfo">radio</p>
                <div class="preview">
                    <span class="inpD">
						<label>
							<input type="radio" name="radio" checked="">
							<i class="ico radio"></i>
							<span class="txt">Newest</span>
                    </label>
                    </span>
                    <span class="inpD">
						<label>
							<input type="radio" name="radio">
							<i class="ico radio"></i>
							<span class="txt">Oldest</span>
                    </label>
                    </span>
                </div>

                <p class="gInfo">radio (.small)</p>
                <div class="preview">
                    <span class="inpD small">
