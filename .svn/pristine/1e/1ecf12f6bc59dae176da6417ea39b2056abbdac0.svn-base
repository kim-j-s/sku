            <section>
                <h2 class="test">text + icon</h2>
                <div>
                    <button type="button" class="btnFilter"><i class="ico filter"></i>Filter<em>(0)</em></button><br>
                    <a href="#" class="btnLink"><span><em>김유니</em>님에게 추천하는 학습 과정 보기</span><i class="ico link"></i></a><br>
                    <a href="#" class="btnLink"><span>Modify</span><i class="ico arrow blue"></i></a><br>
                    <button type="button" class="btnNormal"><i class="ico down red"></i>Report Download</button>
                    <button type="button" class="btnListMore">
                        <i class="ico plus"></i>
                        list more (<em>20</em>/<em>59</em>)
                    </button>
                </div>

            </section>

            <!-- dropdown -->
            <section>
