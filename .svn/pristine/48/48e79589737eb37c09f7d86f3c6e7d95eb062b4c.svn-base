/*common*/
body{background: #f4f7fd;}
.ellipsis2{display: -webkit-box; overflow: hidden; text-overflow: ellipsis; -webkit-line-clamp: 2; -webkit-box-orient: vertical;}
a.linkBlue{color: #0e73db; text-decoration: underline;}
.btnLink{color: #0e73db; font-size: 12px; letter-spacing: -.35px}
.btnLink span{text-decoration: underline; color: #0e73db;}
.btnLink i.ico{vertical-align: baseline;}
.btnLink i.link{margin-left: 2px;}

/*button*/
