/*input*/
.inpD label{position: relative; padding-left: 24px;}
.inpD label input{position: absolute; top: 0; left: 0; opacity: 0; z-index: -1; outline: 0;}
.inpD label .txt{display: inline-block; margin: 1.5px 0 0 10px; font-size: 14px; line-height: 20px; color: #222; vertical-align: top;}

/* checkbox */
.inpD label i.checkBox{position: absolute; top: 0; left: 0; width: 24px; height: 24px; background-image: url(../img/btn_check_nomal.svg);}
.inpD label input[type="checkbox"]:checked ~ .checkBox{background-image: url(../img/btn_check_selected.svg);}
.inpD label input[type="checkbox"]:checked:disabled ~ .checkBox{background-image: url(../img/btn_check_disabled_selected.svg);}
.inpD.black label i.checkBox{background-image: url(../img/btn_check_nomal_black.svg);}
.inpD.black label input[type="checkbox"]:checked ~ .checkBox{background-image: url(../img/btn_check_selected_black.svg);}

/*radio*/
.inpD label i.radio{position: absolute; top: 0; left: 0; width: 24px; height: 24px; background-image: url(../img/btn_radio_nomal.svg);}
.inpD label input[type="radio"] ~ .txt{color: #6c788f;}
.inpD label input[type="radio"]:checked ~ .radio{background-image: url(../img/btn_radio_selected.svg);}
.inpD label input[type="radio"]:checked ~ .txt{font-weight: 700}
.inpD.num label{display: inline-block; padding: 0;}
.inpD.num label .txt{height: 24px; padding: 1px 25px 1px 6px; border: 1px solid #3e8ae9; border-radius: 4px; box-sizing: border-box; margin: 0; min-width: 8px; font-size: 12px; color: #bed4f0; font-weight: 700}
.inpD.num label i.radio{left: inherit; top: 2px; right: 3px; width: 20px; height: 20px;}
.inpD.num label i.radio{background-image: url(../img/icon_check_nomal_40_px.svg);}
.inpD.num label input[type="radio"]:checked ~ .radio{background-image: url(../img/icon_check_selected_40_px.svg);}
.inpD.num label input[type="radio"]:checked ~ .txt{background-color: #3e8ae9; color: #fff;}
.inpD.num label input[type="radio"]:disabled ~ .txt{background-color: #dae0eb; border-color: #dae0eb; color: #b7c0d1;}
.inpD.num label input[type="radio"]:checked:disabled ~ .radio{background-image: url(../img/icon_check_disabled_40_px.svg);}
.inpD.small label i.radio{background-size: 16px; background-position: center}
.inpD.small label input[type="radio"] ~ .txt{margin-left: 1px;}

/* 아코디언 리스트 기본 */
.accordion .viewBox{padding: 12px 15px;}
.accordion .anch{position: relative; display: block; padding: 8px 30px 8px 0; font-size: 14px; line-height: 20.5px; font-weight: 700;}
.accordion .anch strong{font-size: 14px; color: #222;}
.accordion .anch .arrow{position: absolute; top: 6px; right: 0; display: block; width: 25px; height: 25px; background-image: url(../img/icon_arrow_open_50_px.svg); background-size: contain;}
.accordion > li.open .anch .arrow{background-image: url(../img/icon_arrow_close_50_px.svg);}
.accordion > li.open .anch strong{font-weight: 700}

/* popup */
.popWrap{position: absolute; top: 0; left: 0; width: 100%; height: 100%; z-index: 510;}
.popBox{position: absolute; background-color: #fff;}
.popBox .popTitle{position: fixed; width:100%; z-index:3; padding: 15px; border-bottom: 1px solid #b9b9b9; transition:0.3s; background:#fff;}
.popBox .popTitle.on {box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}
.popBox .popTitle p{padding-right: 40px; font-size: 16px; font-weight: 700; color: #222;}
.popBox .popTitle .btnClose{position: absolute; top: 10px; right: 15px; display: block; width: 36px; height: 36px; background: url(../img/icon_close_70_px.svg); background-size: contain; text-indent: -9999px}
.popBox .popTitle.dark{background-color: #45484f; border-color: #646872}
.popBox .popTitle.dark p{color: #fff;}
.popBox .popTitle.dark .btnClose{background: url(../img/icon_close_w_70_px.svg); background-size: contain;}
.popBox .popCont > .inner{padding: 30px 15px 0; font-size: 14px; line-height: 22px;}
.popBox .popCont > .inner b{display: block; margin-bottom: 5px; color: #222;}

.full.popWrap{position: fixed;}
.full .popBox{top: 0; left: 0; width: 100%; height: 100%;}
.full .popBox .popCont{padding-bottom: 100px;}
.full .popBox .popCont.noPad{padding: 0;}
.full .popBox .popTitle + .popCont{height: 100%; padding-top:55px; overflow: auto; overflow-x:hidden; -webkit-overflow-scrolling: touch;}

.center.popWrap:before{display: block; content: ''; width: 100%; height: 100%; position: fixed; top: 0; left: 0; background-color: rgba(0,0,0,.7);}
.center .popBox{top: 115px; left: 50%; width: 80%; transform: translate(-50%, 0); border-radius: 10px; overflow: hidden;}
.center .popBox .popCont{padding: 20px 20px 30px;}
.center .popBox .scrollArea{min-height: 76px; height: 76px; overflow: hidden;}
.center .popBox strong.title{display: block; margin-bottom: 20px; font-size: 14px; color: #222; font-weight: 700; letter-spacing: -.3px}
.center .popBox p.txt{font-size: 12px; line-height: 19px; letter-spacing: -.2px;}
.center .popBox .popBtm{display: flex; justify-content: space-between; border-top: 1px solid #ddd}
.center .popBox .popBtm button{padding: 14px 20px 15px; font-size: 14px;}
.center .popBox .popBtm button:last-of-type{color: #e1002a;}
.center .popBox .popBtm button:only-child{width: 100%; color: #e1002a;}
.center.on .popBox{top: 50%; left: 50%; width: 80%; transform: translate(-50%, -50%); border-radius: 10px; overflow: hidden;}
.center.on .popBox .scrollArea{max-height: 50vh; height:auto;}

/* 별점 팝업 */
.popBox .topView .title{padding-right: 0;}
.popBox .topView + .starRate{padding-top: 40px;}

/* 클래스 정보 팝업 */
.topDesc{padding: 15px; color: #6c788f; font-size: 12px;}
.classTableWrap .line{position: relative; display: flex; align-items: center; width: 100%; line-height: 24px;}
.classTableWrap .line > .num{white-space: nowrap; min-width: 50px; font-weight: 700}
.classTableWrap .line > .name{padding-right: 10px; letter-spacing: -.3px; text-overflow: ellipsis; overflow: hidden;}
.classTableWrap .line > .date{flex: none; width: 135px; margin-left: auto;}
.classTableWrap .line > .anch{flex: none; position: absolute; top: 0; right: 0; width: 55px; height: 58px; padding: 0; text-align: right; background: url(../img/icon_arrow_open_50_px.svg) no-repeat center; background-size: 25px;}
.tableHead{display: flex; padding: 15px 60px 10px 15px; color: #999; font-size: 12px; letter-spacing: -.2px; font-weight: 700}
.classInfo{width: 100%; border-top: 1px solid #ddd; text-align: left;}
.classInfo li{border-bottom: 1px solid #ddd}
.classInfo li.open .line{font-weight: 700}
.classInfo li.open .anch{background-image: url(../img/icon_arrow_close_50_px.svg);}
.classInfo li.disabled{color: #b9b9b9;}
.classInfo .line{padding: 17px 60px 17px 15px; font-size: 14px;}
.classInfo .line .inpD.num{display: inline-block;}
.classInfo .line .inpD.num label{display: inline-block; vertical-align: top;}
.classInfo .sub{padding: 22px 18px; margin-bottom: -1px; background-color: #f4f7fd; border-top: 1px solid #a8b5cd; border-bottom: 1px solid #a8b5cd;}
.classInfo .sub .txtTable{font-size: 14px;}
.classInfo .sub .txtTable th{white-space: nowrap; vertical-align: top;}
.classInfo .sub .txtTable th, .classInfo .sub .txtTable td{padding: 8px 0;}

.topAllSel{padding: 15px 15px 10px;}
.topAllSel .inpD.black .txt{font-weight: 700; color: #999; font-size: 12px}
.downloadList{border-top: 1px solid #ddd;}
.downloadList li{padding: 17px 15px; border-bottom: 1px solid #ddd; white-space: nowrap;}
.downloadList .inpD label{display: block; vertical-align: top;}
.downloadList .inpD span.txt{display: block; margin: 0 0 0 10px; text-overflow: ellipsis; overflow: hidden; line-height: 24px; letter-spacing: -.3px}
.downloadList .inpD.checked span.txt{font-weight: 700}

.BtmBtn{width: 100%;}
.BtmBtn .stateTxt{padding: 6px 15px; font-size: 12px; line-height: 18px; color: #ff664d; font-weight: 700; text-align: center; background-color: #fff;}
.BtmBtn .stateTxt:only-child{padding: 11px 15px;}
.BtmBtn .btnGroup{display: flex;}
.BtmBtn .btnGroup > *{height: 44px; flex: 1; font-size: 14px; font-weight: 700; color: #fff; -webkit-tap-highlight-color: transparent; user-select: none;}
.BtmBtn .btnGroup .btnOrange{background-color: #ff664d; transition: all .1s}
.BtmBtn .btnGroup .btnBlack{background-color: #444; transition: all .1s}
.BtmBtn .btnGroup .btnOrange:active{background-color: #b2412f}
.BtmBtn .btnGroup .btnBlack:active{background-color: #222}
.BtmBtn.floating{position: fixed; bottom: 0; left: 0; z-index: 3; box-shadow: 0px -1px 4px rgba(0,0,0,0.2)}
.BtmBtn.floating.on {position:static;}

/* card */
.card{display: block; margin: 0 15px 15px; padding: 24px 20px 0; background-color: #fff; box-shadow: 0px 1px 5px rgba(204,212,231,.8); border-radius: 10px; overflow: hidden;}
.card .cardInner{position: relative;}
.card .titleArea{display: block; margin-bottom: 10px;}
.card .titleArea .title{display: -webkit-box; height: 54px; padding-right: 20px; font-size: 18px; color: #222; font-weight: 700; line-height: 27px; letter-spacing: -.45px; 	overflow: hidden; text-overflow: ellipsis; 	-webkit-line-clamp: 2; -webkit-box-orient: vertical; word-wrap:break-word;}
.card .infoArea{width: 100%; font-size: 14px; line-height: 1; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.card .btmArea{display: flex; justify-content: space-between; align-items: center; margin: 20px -20px 0px; padding: 12px 20px 15px; border-top: 1px solid #eee; font-size: 12px; color: #666;}
.card .category{margin-bottom: 25px;}
.card .icon{position: absolute; top: -4px; right: 0; font-size: 10px; font-weight: 700; color: #222; line-height: 27.5px;}
.card .icon i{width: 27.5px; height: 27.5px; margin-left: 5px; vertical-align: top;}
.card .label{display: inline; vertical-align: top;}
.card .label i{width: 16px; height: 16px; margin: 2px 2px 0 0; vertical-align: top;}
.card .label b{color: #6c788f; font-size: 14px; line-height: 20px; vertical-align: top;}
.card .label + .label{margin-left: 6px;}
.card .tag{padding-top: 3px; color: #6280b2;}
.card .tag span{line-height: 1.45;}
.card .tag + .date{margin-top: 2px;}
.card .date{font-size: 12px; line-height: 1.5; color: #666; letter-spacing: 0.5px}
.card .state{color: #222;}
.card i.check{margin-right: 3px;}

/* course card */
.cardWrap{margin: 0 15px; background-color: #fff; box-shadow: 0px 1px 5px rgba(204,212,231,.8); border-radius: 10px; overflow: hidden;}
.cardWrap .cardToggle{position: absolute; right: 0; bottom: 0; width: 30px; height: 30px; background: url(../img/btn_card_close.svg); background-size: contain; transform: rotate(180deg);}
.cardWrap.open .cardToggle{transform: rotate(0);}
.cardWrap .card{display: block; margin: 0; border-radius: 0; box-shadow: none;}
.cardWrap .card + .card{border-top: 1px solid #eee}
.cardWrap .card .btmArea{border-top: 0; padding-bottom: 16px;}
.cardWrap .card.course ~ .card{display: none;}
.card.course{padding-bottom: 24px;}
.card.course .category{margin-bottom: 29.5px;}
.card.course .infoArea{padding-right: 30px;}

/* card college tag color */
.category{display: inline-block; min-width: 30px; padding: 1.5px 6px 2.5px; font-size: 10px; line-height: 15px; font-weight: 700; color: #fff; letter-spacing: -.08px; border-radius: 2px; vertical-align: top; text-align: center;}
.category.red{background-color:#db2828;}
.category.orange{background-color:#f2711c;}
.category.yellow{background-color:#ffb306;}
.category.teal{background-color:#00b5ad;}
.category.green{background-color:#21ba45;}
.category.blue{background-color:#2185d0;}
.category.violet{background-color:#6435c9;}
.category.purple{background-color:#9f3fc5;}

/* 별점 */
.starRate{font-size: 0; line-height: 1; text-align: center;}
.starRate i{display: inline-block; width: 16px; height: 16px; line-height: 0; background-image: url(../img/icon_star_basic_32_px.svg); background-repeat: no-repeat; background-size: contain; vertical-align: top;}
.starRate i + i{margin-left: 2px;}
.starRate.s1 i:nth-child(-n+1){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s2 i:nth-child(-n+2){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s3 i:nth-child(-n+3){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s4 i:nth-child(-n+4){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.s5 i:nth-child(-n+5){background-image: url(../img/icon_star_sel_32_px.svg)}
.starRate.w70 i{width: 35px; height: 35px; background-image: url(../img/icon_star_basic_70_px.svg)}
.starRate.w70 i + i{margin-left: 15px;}
.starRate.w70 i.on{background-image: url(../img/icon_star_sel_70_px.svg)}


/* 상단 필터버튼 영역 */
.topGuide{display: flex; justify-content: space-between; padding: 30px 15px 0; margin: 0 0 15px;}
.topGuide .txt{font-size: 12px; line-height: 30px; color: #6c788f; letter-spacing: -0.25px}
.btnFilter{height: 30px; padding: 0 7.5px 0 6px; border: 1px solid #6c788f; border-radius: 4px; font-size: 14px; font-weight: 700; line-height: 20px; letter-spacing: -.26px; color: #6c788f; vertical-align: top;}
.btnFilter i.filter{margin-right: 3px; vertical-align: top;}
.btnFilter em{margin-left: 0.5px; font-size: 12px; font-weight: 700; letter-spacing: -.15px; vertical-align: top;}

/* sub tab menu */
.tabBox{height: 56px;}
.tabBox .menuWrap{position: relative; width: 100%; height: 56px; background-color: #fff; border-bottom: 1px solid #c8d0e0; overflow-y: hidden; z-index: 1; transition:0.3s;}
.tabBox .menuWrap.on{position: fixed; box-shadow: 0px 3px 10px 0px rgba(35,46,62,0.2);}
.menu{font-size: 0; white-space: nowrap; overflow-x: auto; padding-bottom: 20px;}
.menu > *{position: relative;}
.menu a{display: inline-block; margin: 0 15px; padding: 17px 0 15px; line-height: 20px; font-size: 14px; color: #222; border-bottom: 3px solid #fff}
.menu a .count{display: inline-block; margin-left: 3px; font-size: 12px; line-height: 1; font-weight: 700; color: #999; vertical-align: baseline;}
.menu a + a{margin-left: 20px;}
.menu a.active{font-weight: 700; border-color: #e1002a}
.menu a.active .count{color:#e1002a;}
.menu a:first-of-type, 
.menu a:last-of-type{z-index: 3;}
.menu:before, 
.menu:after{display: block; content: ''; position: absolute; top: 0; width: 36px; height: 55px; z-index: 2;}
.menu:before{left: 0; background: linear-gradient(to right,  rgba(255,255,255,1) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,0) 100%);}
.menu:after{right: 0; background: linear-gradient(to right,  rgba(255,255,255,0) 0%,rgba(255,255,255,0.7) 50%,rgba(255,255,255,1) 100%);}

.emptyBox{padding: 120px 30px 70px; text-align: center;}
.emptyBox i.noCont{flex: none; display: block; margin: 0 auto 14px; }
.emptyBox .txt{display: block; font-size: 14px; color: #6c788f; letter-spacing: -.35px; line-height: 1.45;}
.emptyBox .sub{margin: 4px 0 0; font-size: 12px; line-height: 19px; letter-spacing: -.2px; color: #6c788f}
.emptyBox .sub + .btnLink{margin-top: 12px;}
.emptyBox .btnLink{display: inline-block; margin-top: 4px;}
.emptyBox.noSch .txt{font-size: 12px;}
.emptyBox.noSch .txt strong{display:block; padding:10px 0 5px 0; font-weight:700; font-size: 14px;}
.emptyBox.sch .txt{font-size: 12px;}
.emptyBox.sch .txt strong{display:block; padding:5px 0 5px 0; font-weight:700; font-size: 14px; color:#8690a4;}
.tabCont .emptyBox{padding-top: 60px;}
.cmntEnter + .emptyBox{margin-top: 0; padding-top: 50px;}

.filterList > li{border-bottom: 1px solid #ddd}
.filterList .line{display: flex; justify-content: space-between;}
.filterList .selectedItem{white-space: nowrap; font-size: 12px; color: #3e8ae9; overflow: hidden; text-overflow: ellipsis;}
.filterList .selectedItem span:after{display: inline-block; content: ',';}
.filterList .selectedItem span:last-of-type:after{display: none;}
.filterList .count{flex: none; display: inline-block; min-width: 30px; margin-left: 10px; color: #4c4c4c; font-size: 12px; font-weight: 700; line-height: 18px; text-align: center; background-color: #eee; border-radius: 4px;}
.filterList .sub{padding: 29px 15px; background-color: #f4f7fd; border-top: 1px solid #a8b5cd; border-bottom: 1px solid #a8b5cd; margin-bottom: -1px;}
.filterList .sub li + li{margin-top: 30px;}
.filterList .filterListName {padding:11px 15px; font-weight:700; font-size:12px; color:#6c788f; line-height:18px; background:#f9f9f9;}

table.txtTable{text-align: left; font-size: 12px;}
table.txtTable th{color: #6c788f;}
table.txtTable th span{position: relative;}
table.txtTable th span:before{display: inline-block; content: ''; width: 3px; height: 3px; margin: 8px 3px 0 0; border-radius: 50%; background-color: #6c788f; vertical-align: top;}
table.txtTable td{color: #222;}
table.txtTable th, 
table.txtTable td{padding: 5px 0;}

table.borderTable{width: 100%; font-size: 12px; color: #282e3b; border-top: 3px solid #c9d3e6; border-bottom: 3px solid #c9d3e6; text-align: center; table-layout: fixed;}
table.borderTable th{padding: 10px 0;}
table.borderTable td{padding: 10px 0;}
table.borderTable tbody tr{border-top: 1px solid #c9d3e6}
table.borderTable td p{white-space: nowrap; text-overflow: ellipsis; overflow: hidden;}

.topView{position: relative; min-height: 270px; padding: 30px 25px; background-color: #45484f; color: #fff; overflow: hidden;}
.topView .ribbon{position: absolute; top: 0; right: 0; width: 65px; height: 65px; transform: rotate(45deg);}
.topView .ribbon span{position: absolute; bottom: 50%; left: 50%;	display: block; min-width: 100px; transform: translate(-50%); font-size: 10px; line-height: 24px; background-color: #6280b2; text-align: center; box-shadow: 0px 1px 5px rgba(0,0,0,0.3)}
.topView .category{margin-bottom: 10px;}
.topView .title{display: block; min-height: 96px; font-size: 20px; line-height: 32px; font-weight: 700; letter-spacing: -.5px;}
.topView .infoArea{display: flex; margin-top: 35px; font-size: 0; line-height: 24.5px; letter-spacing: -.25px;}
.topView .infoArea >  *{line-height: 20px; vertical-align: bottom; white-space: nowrap;}
.topView .infoArea span{font-size: 14px;}
.topView .infoArea .icon{flex: none; position: relative; display: inline-block; padding-left: 30.5px; margin-right: 5px; font-weight: 700}
.topView .infoArea .icon i.ico{position: absolute; bottom: -2px; left: 0; width: 27.5px; height: 27.5px;}
.topView .infoArea .icon:after{display: inline-block; content: ''; width: 1px; height: 8px; margin-left: 5px; background-color: #969dac;}
.topView .infoArea .icon + span{flex: auto; text-overflow: ellipsis; overflow: hidden;}
.topView .infoArea .stamp{flex: none; margin-left: auto; font-weight: 700}
.topView .infoArea i.stamp2{width: 16px; height: 16px; margin: 2px 2px 0 0; background-image: url(../img/icon_stamp_detail_32_px.svg); vertical-align: top;}
.topView .date{margin-top: 10px; font-size: 12px; color: #adadad;}

.videoWrap{position: relative; padding: 0 0 56.2%}
.videoWrap iframe{position: absolute; top: 0; left: 0; display: block; width: 100%; height: 100%;}

.descWrap{padding: 25px 15px 30px; background-color: #fff; font-size: 12px; border-bottom: 1px solid #ebebeb;}
.timeTable{padding-bottom: 15px;}

.chargeInfo{padding-top: 20px; border-top: 1px solid #eee}
.chargeInfo > p{letter-spacing: -.2px}
.chargeInfo > p > span{display: inline-block; line-height: 20px; vertical-align: top}
.chargeInfo > p > span em + em{margin-left: 5px;}
.chargeInfo > p > span em + em:before{display: inline-block; content: ''; width: 1px; height: 8px; margin-right: 5px; background-color: #c8d0e0}
.chargeInfo > p + p{margin-top: 10px;}
.chargeInfo a{color: #0e73db; text-decoration: underline; letter-spacing: 0}

.tabCont{padding: 30px 15px 100px;}
.tabCont.noPad{padding-top: 0; padding-right: 0; padding-left: 0;}
.tabCont > * + *{margin-top: 30px;}
.tabCont .btnListMore{margin-bottom: 0;}
.tabCont .cardWrap, .tabCont .cardBox > .card{margin: 0 0 15px;}
.tabCont .cardBox > *:last-child{margin-bottom: 0;}
.tabCont:last-child{padding-bottom: 60px;}

.titleDot{display: block; font-size: 14px; font-weight: 700; color: #282e3b;}
.titleDot:before{display: inline-block; content: ''; width: 3px; height: 3px; margin: 9px 3px 0 0; border-radius: 50%; background-color: #282e3b; vertical-align: top;}

.presentText .flowText{max-height: 88px; overflow: hidden;}
.presentText .flowText.opened{max-height: inherit;}
.presentText .txtCont{font-size: 14px; line-height: 22px; color: #282e3b; letter-spacing: -.3px; }
.presentText .link{margin-top: 20px;}
.presentText .link .titleDot{margin-bottom: 4px;}
.presentText .link a{display: block; font-size: 14px; line-height: 22px; color: #0e73db; word-break: break-all; text-decoration: underline;}
.presentText .moreRead{display: block; width: 100%; margin: 15px auto 0; padding: 0 20px; font-size: 12px; color: #6c788f;}
.presentText .moreRead:after{display: inline-block; content: ''; width: 0; height: 0; margin-left: 3px; border-top: 7px solid #6c788f; border-right: 5px solid transparent; border-left: 5px solid transparent;}

.downLink{display: block; padding: 12px 75px 12px 15px; background: #fff url(../img/icon_download_50_px.svg) no-repeat center right 25px; background-size: 25px auto; border-radius: 10px; box-shadow: 0px 1px 5px rgba(204,212,231,0.3);}
.downLink .text{display: block; font-size: 12px; color: #6c788f;}

.topRight{display: inline-block; float: right; margin: -24px 0 0;}
.classTable .borderTable{margin-top: 15px;}

.introList{background-color: #fff; border-radius: 10px; box-shadow: 0px 1px 5px rgba(204,212,231,0.8);}
.introList > li{padding-bottom: 9px;}
.introList > li + li{border-top: 1px solid #ddd}
.introList .viewBox{padding-bottom: 3px;}
.introList .sub{padding: 0 15px 18px; font-size: 14px; line-height: 22px; letter-spacing: -.3px}

.keywordWrap{font-size: 0; margin-bottom: -10px;}
.keywordWrap > span{display: inline-block; padding: 0 11px; margin: 0 5px 10px 0; font-size: 14px; line-height: 23px; color: #6c788f; background-color: #fff; border: 1px solid #d1d6e2; border-radius: 13px;}

.registWrap{display: flex; line-height: 22px; font-size: 0}
.registWrap > *{display: inline-block; vertical-align: top;}
.registWrap .starRate{padding-top: 5px;}
.registWrap .score{margin-left: 9px; font-size: 14px;}
.registWrap .score strong{display: inline-block; margin-right: 2px; font-weight: 700; font-size: 22px; vertical-align: bottom;}
.registWrap .btnLink{margin-left: auto; font-weight: 700;}

.sortArea{margin-top: 30px; padding: 0 0 15px; font-size: 0;}
.sortArea .inpD{display: inline-block;}
.sortArea .inpD +.inpD{margin-left: 20px;}
.sortArea .inpD label{display: block; height: 24px;}
.sortArea .inpD .txt{margin: 0 0 0 5px; line-height: 24px;}
.content > .sortArea{padding-right: 15px; padding-left: 15px;}


/************ comment **********/

.cmntEnter{padding: 10px 0;}
.cmntEnter .inner{background-color: #fff; border: 1px solid #b9b9b9; border-radius: 4px; overflow: hidden;}
.cmntEnter .field{height: 110px; padding: 12px 15px; overflow-y: auto;}
.cmntEnter .field textarea{display: block; width: 100%; min-height: 22px; border: 0 none; font-size: 14px; line-height: 22px; color: #4c4c4c;}
.cmntEnter .field textarea:only-child{margin: -12px -15px; padding: 12px 15px; height: 110px; width: calc(100% + 30px)}
.cmntEnter .field .addPic{position: relative; width: 180px; margin-top: 10px; font-size: 0;}
.cmntEnter .field .addPic img{width: 100%;}
.cmntEnter .field .addPic .btnImgDel{position: absolute; top: 0; left: 0;}
.cmntEnter .enter{display: flex; border-top: 1px solid #b9b9b9}
.cmntEnter .enter > * + *{border-left: 1px solid #b9b9b9}
.cmntEnter .enter .area{flex: 1; padding: 0 15px; text-align: right;}
.cmntEnter .enter .byte{display: inline-block; padding: 5px 0; font-size: 14px; line-height: 30px; color: #999;}
.cmntEnter .enter .byte .left{color: #222;}
.cmntEnter .enter .btn{flex: none; padding: 0 15px; font-size: 14px; font-weight: 700; color: #4c4c4c; border: 0 none; background-color: transparent; border-left: 1px solid #b9b9b9;}
.cmntEnter .enter .btn:disabled{color: #b9b9b9;}
.cmntEnter.focus .inner,
.cmntEnter.focus .enter,
.cmntEnter.focus .enter > * + *{border-color: #6ea6ec; transition: .2s}
.cmntEnter.txtOver .enter .byte .left{color: #e1002a;}
.cmntEnter.txtOver .enter .btn{color: #b9b9b9; -webkit-tap-highlight-color: transparent;}

.cmntArea{background-color: #fff; margin: 0 -15px; padding: 0 15px; border-bottom: 1px solid #c8d0e0}
.cmntArea > li{padding-top: 20px;}
.cmntArea > li > .box{padding: 0 0 0 50px;}
.cmntArea li + li{border-top: 1px solid #ddd}
.cmntArea li .box{position: relative;}
.cmntArea li.deleted .box{padding-bottom:20px;}
.cmntArea .cmntBtn{position: absolute; top: 0; right: 0;}
.cmntArea .cmntEnter{margin: 15px 0 0 -50px; padding: 0;}
.cmntArea .cmntEnter + .cmntText{display: none;}
.cmntArea ul.reply .cmntEnter{margin: 11px 0 0-42px;}
/* 작성정보 */
.cmntArea .pic{position: absolute; top: 0; left: 0; display: block; width: 40px; height: 40px; border-radius: 50%; overflow: hidden;}
.cmntArea .pic img{width: 100%;}
.cmntArea .info{height: 40px; padding: 2px 110px 0 0; font-size: 0; white-space: nowrap;}
.cmntArea .info .name{display: inline-block; font-size: 14px; font-weight: 700}
.cmntArea .info .date{display: inline-block; margin-left: 6px; font-size: 14px; color: #666;}
.cmntArea .info .date:before{display: inline-block; content: ''; width: 1px; height: 8px; margin: 7px 6px 0 0; background-color: #ddd; vertical-align: top;}
/* 버튼 */
.cmntBtn{font-size: 0}
.cmntBtn .btn + .btn{margin-left: 15px;}
.cmntBtn .btn{width: 24px; height: 24px; background-size: contain;}
.cmntBtn .btn.rep{background-image: url(../img/icon_comment_reply_48_px.svg);}
.cmntBtn .btn.del{background-image: url(../img/icon_comment_delete_48_px.svg);}
.cmntBtn .btn.edit{background-image: url(../img/icon_comment_edit_48_px.svg);}
.moreReply{display: block; padding-right: 17px; font-size: 12px; line-height: 18px; color: #666; background: url(../img/icon_comment_open_28_px.svg) no-repeat right top 3px; background-size: 14px; text-align: left;}
.moreReply.opened{background-image: url(../img/icon_comment_close_selected_28_px.svg);}
.moreReply em{display: inline-block; font-size: 12px; font-weight: 700; color: #ff664d; vertical-align: top;}
/* 댓글 본문 텍스트 */
.cmntArea .flowText{position: relative;}
.cmntArea .cmntText .txtCont{font-size: 14px; line-height: 22px; word-break: break-all; letter-spacing: -.3px; text-align: left;}
.cmntArea .cmntText.opened{max-height: inherit; padding-bottom: 0;}
.cmntArea .cmntText.opened .txtCont{overflow: inherit; -webkit-line-clamp: inherit; -webkit-box-orient: vertical;}
.cmntArea .moreReply{margin-top:15px; }
/* 첨부 이미지 */
.cmntArea .img{display: block; margin: 35px 0 15px; width: 157px;}
.cmntArea .img img{width: 100%; vertical-align: top;}
.cmntArea .img + .moreReplyArea .moreRead {padding-top:0;}
.cmntArea .img + .moreReplyArea .moreReply {margin-top:0;}
.cmntArea .opened + .img{margin-top: 16px;}
/* 대댓 보기 버튼 */
.cmntArea .moreReplyArea {padding-bottom:20px; overflow:hidden;}
.cmntArea .moreReplyArea .moreRead {float:right; font-size: 12px; color: #0e73db; vertical-align: top; padding-top:15px;}
/* 대댓 */
.cmntArea ul.reply{padding-left: 50px; border-top: 1px solid #ddd}
.cmntArea ul.reply li{padding: 20px 0; }
.cmntArea ul.reply .box{padding: 0 0 0 42px;}
.cmntArea ul.reply .pic{width: 32px; height: 32px;}
.cmntArea ul.reply .info{height: 32px;}
.cmntArea ul.reply .cmntText{margin-top: 4px; padding-bottom: 0;}
.cmntArea ul.reply .txtCont b{display: inline-block; margin-right: 3px; color: #51aaff;}
/* 삭제된 댓글 */
.cmntArea li.deleted > .box .pic{background: url(../img/icon_noti_80_px.svg) no-repeat; background-size: contain;}
.cmntArea li.deleted > .box .info{padding: 0; font-size: 14px; line-height: 40px;}
.cmntArea ul.reply li.deleted > .box .info{line-height: 32px;}

.tagMenu{position: fixed; left: 0; bottom: 0; width: 100%; max-height: 240px; overflow-y: auto; background-color: #fff; z-index: 4; box-shadow: 0px -1px 4px rgba(0,0,0,0.2)}
.tagMenu .item{display: block; position: relative; width: 100%; padding: 9px 15px 9px 64px; border-bottom: 1px solid #ddd;}
.tagMenu .item .pic{position: absolute; top: 9px; left: 15px; width: 40px; height: 40px;}
.tagMenu .item .pic img{width: 100%;}
.tagMenu .item p{font-size: 12px; text-align: left; line-height: 20px;}
.tagMenu .item .name{margin-right: 5px; font-size: 14px; font-weight: 700;}
.tagMenu .item .name em{font-weight: 700; color: #51aaff;}
.tagMenu .item .work{color: #666;}
.tagMenu .item .mail{font-weight: 700; color: #4c4c4c;}
.tagMenu .item.active{background-color: #f4f7fd}



/*툴팁*/
.tooltip {position: relative; display: inline-block; vertical-align: top; font-size: 12px}
.tooltip .ico.tip{margin-top: 1px; vertical-align: top;}
.tooltip .tipBox{position: absolute; left: 0; bottom: 100%; min-width: 330px; margin: 0 0 20px -42.5px;}
.tooltip .tipBox .inner{position: relative; padding: 15px; color: #fff; background-color: rgba(34,34,34,0.97); border-radius: 10px; box-shadow: 0px 3px 6px rgba(34,34,34,.97); z-index: 1;}
.tooltip .tipBox:before{position: absolute; left: 42px; /* top: 100%; */ bottom:-16px; display: block; content: ''; width: 20px; height: 16px;  /* box-shadow: 0px 3px 6px rgba(34,34,34,.7); */ background: url(../img/icon_tri_40_x_32_px.svg) no-repeat; background-size: contain;}
.tooltip .tipBox strong{display: block; padding-right: 20px; margin-bottom: 18px; font-size: 14px; font-weight: 700}
.tooltip .tipBox p{line-height: 19px;}
.tooltip .tipBox p + p{margin-top: 3px;}
.tooltip .closeTip{position: absolute; top: 15px; right: 15px; display: block; width: 20px; height: 20px; background: url(../img/btn_close_w_40_px.svg) no-repeat; background-size: contain;}

.step{font-size: 0}
.step b{font-size: 12px;}
.step b + b:before{display: inline-block; content: ''; width: 3.5px; height: 3.5px; margin: 7.5px 9px 0 6px; border-right: 1px solid #fff; border-top: 1px solid #fff; transform: rotate(45deg); vertical-align: top;}

/*게시판*/
.postInfo > *{display: inline-block; margin-right: 6px; font-size: 12px; color: #666; white-space: nowrap;}
.postInfo i.view{margin-right: 2px; vertical-align: top;}
.postInfo .new{font-weight: 700; color: #ff664d;}
.postInfo .reply{margin-left: auto; margin-right: 0;}
.postInfo .reply em{display: inline-block; margin-right: 3px; font-size: 13px; font-weight: 700; line-height: 18px; color: #4c4c4c; vertical-align: top;}
.postInfo .cmntBtn{margin-left: auto; margin-right: 0; font-size: 0;}

/*게시판 리스트*/
.boardList{margin-top: 5px; background-color: #fff; border-bottom: 1px solid #c8d0e0}
.boardList li + li{border-top: 1px solid #ddd}
.boardList li a{position: relative; display: flex; align-items: center; padding: 15px 0;}
.boardList .inner{flex: auto; padding: 0 15px; overflow: hidden;}
.boardList .title{height: 40px; display: -webkit-box; overflow: hidden; text-overflow: ellipsis; -webkit-line-clamp: 2; -webkit-box-orient: vertical; line-height: 20px;}
.boardList .title .redDot{vertical-align: top; margin: 6px 3px 0 0;}
.boardList .title{line-height: 20px; font-size: 14px; font-weight: 400; word-break: break-all; vertical-align: top}
.boardList .postInfo{margin-top: 20px; font-size: 0}
.boardList .reply{flex: none; min-width: 62px; padding: 20px 5px; font-size: 12px; line-height: 1; color: #888; border-left: 1px solid #ddd; text-align: center; vertical-align: top;}
.boardList .reply em{display: block; margin-bottom: 7px; font-size: 13px; font-weight: 700; color: #4c4c4c;}
.boardList .subList{background-color: #fafcff; border-top: 1px solid #ddd}
.boardList .subList li:last-child{border-bottom: 0 none;}
.boardList .subList li .inner{position: relative; padding-left: 26px;}
.boardList .subList .titleWrap{position: relative;}
.boardList .subList .titleWrap:before{display: block; content: ''; position: absolute; top: 5px; right: 100%; width: 6px; height: 6px; margin-right: 5px; border-left: 1px solid #6280b2; border-bottom: 1px solid #6280b2; box-sizing: border-box;}

/*게시글*/
.boardDetail{padding: 30px 0 50px; background-color: #fff;}
.boardDetail .topText{padding: 0 15px 15px; border-bottom: 1px solid #ebebeb;}
.boardDetail .topText .cate{display: block; margin-bottom: 3px; font-size: 12px; letter-spacing: -.2px; color: #6280b2;}
.boardDetail .titleWrap .redDot{margin-top: 9px; vertical-align: top;}
.boardDetail .titleWrap .title{font-weight: 700; font-size: 18px; color: #222; line-height: 27px; letter-spacing: -.45px; word-break: break-all;}
.boardDetail .postInfo{display: flex; align-items: center; margin-top: 25px;}
.boardDetail .contText{padding: 20px 15px 0;}
.boardDetail .contText > p{color: #4c4c4c; font-size: 14px; line-height: 22px; letter-spacing: -.4px; word-break: break-all;}
.boardDetail .contText > img{max-width: 100%; vertical-align: top;}
.boardDetail .contText > * + *{margin-top: 15px;}

/*게시판 댓글*/
.boardComment{padding: 0 15px 60px; border-top: 1px solid #c8d0e0}
.boardComment .cmntEnter{margin-top: 20px;}
.boardComment .btnNormal{margin-top: 30px;}
.boardComment .btnListMore{margin-bottom: 0;}

/* 피드 */
.feedWrap{padding: 30px 0 0;}
.feedWrap .card{position: relative; padding: 20px 20px 24px;}
.feedWrap .card a.titleArea{display: block; margin: 0 20px 0 0;}
.feedWrap .card .title{padding: 0;}
.feedWrap .card .title > b{color: #ff664d;}
.feedWrap .card .postTitle{padding-right: 18px;}
.feedWrap .card .postTitle > span{position: relative; display: inline-block; max-width: 100%; padding: 0 6.4px; margin-bottom: 5px; font-size: 14px; color: #6280b2; letter-spacing: -.35px; vertical-align: top; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; box-sizing: border-box;}
.feedWrap .card .postTitle > span:before,
.feedWrap .card .postTitle > span:after{position: absolute; top: 0; display: block;}
.feedWrap .card .postTitle > span:before{left: 0; content: '“';}
.feedWrap .card .postTitle > span:after{right: 0; content: '”';}
.feedWrap .card .addInfo{display: flex; margin-top: 15px; font-size: 0; white-space: nowrap;}
.feedWrap .card .addInfo span{display: inline-block; font-size: 12px; color: #888; letter-spacing: 0}
.feedWrap .card .addInfo span + span{margin-left: 5px;}
.feedWrap .card .addInfo span + span:before{display: inline-block; content: ''; width: 1px; height: 8px; margin-right: 5px; background-color: #c8d0e0}
.feedWrap .card .addInfo .date{flex: none;}
.feedWrap .card .addInfo .name{flex: auto; overflow: hidden; text-overflow: ellipsis;}
.feedWrap .card .btnDelFeed{position: absolute; top: 15px; right: 15px;}

/* 커뮤니티 카드 리스트 */

.cardBoardList .card .category{margin-bottom: 15px;}
.cardBoardList .card .noCont{margin: 20px -20px 0; padding: 15px 20px; color: #6c788f; font-size: 14px; border-top: 1px solid #ddd}

.card .boardList{margin: 20px -20px 0; border-top: 1px solid #ddd}
.card .boardList li:last-child{border-bottom: 0;}
.card .boardList .inner{padding: 0 14px 0 20px;}
.card .boardList .title{display: block; height: auto; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;}
.card .boardList .reply{min-width: 53px; padding: 3px 6px 3px 4px;}
.card .boardList .postInfo{margin-top: 8px;}
.card .boardList .date{color: #888; letter-spacing: 0;}

.commu .boardList a{padding: 9px 0;}
.commu .boardList .title{max-height: 40px; height: auto;}
.commu .boardList .inner{padding: 7px 15px}
.commu .boardList .reply{padding: 20px 5px;}


