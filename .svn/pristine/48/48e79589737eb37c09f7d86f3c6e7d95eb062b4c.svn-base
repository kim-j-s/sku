.btnPhoto{width: 40px; height: 40px; background: url(../img/photo.svg) no-repeat center top 1px; background-size: contain;}
.btnImgDel{width: 24px; height: 24px; background: url(../img/btn_imgdelete_48_px.svg) no-repeat; background-size: contain;}

